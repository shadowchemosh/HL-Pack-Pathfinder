  <thing id="pS2ClsRng" name="Class Ability: Range" description="Select this adjustment to adjust class ability range value up or down." compset="InPlay">
    <usesource source="srcS2Adjus"/>
    <tag group="Helper" tag="CoreCampOK"/>
