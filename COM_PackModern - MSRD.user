perform hero.assign[WepProf.wMSRDLaOpt]</eval>
    <eval phase="PostAttr" priority="10000" index="4"><![CDATA[      if (field[abValue].value >= 2) then
        ~ this needs the tag for an extra cybertech eye slot
        perform hero.assign[ExtraSlot.EqpEyeCy]
endif]]></eval>
    <eval phase="First" priority="550" index="3"><![CDATA[      foreach pick in hero from BaseWonder where "thingid.ioMSRDLsOp"
        if (eachpick.field[gIsEquip].value <> 0) then
          field[abValue].value += 1
          endif
        nexteach

      if (field[abValue].value >= 2) then
        perform hero.assign[Custom.MSRDLsEye1]
        perform hero.assign[Custom.MSRDLsEye2]
      elseif (field[abValue].value >= 1) then
        perform hero.assign[Custom.MSRDLsEye1]
