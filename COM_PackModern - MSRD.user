  <thing id="raMSRDPMag" name="Psionics" description="{b}Benefit{/b}: The extraterrestrial gains the use of one psionic power of 2nd level or lower. It can use this power three times per day.\n\n{b}Note{/b}: You also gain access to occult skill unlocks for any skills in which you have ranks. If you have no ranks in the appropriate skill, you can’t use the occult skill unlock, even if that skill can be used untrained." compset="RaceSpec" summary="Use of one psionic power of 2nd level or lower.">
    <usesource source="GRCMP2ScF"/>
    <usesource source="MSRDFutur7"/>
    <usesource source="MSRDFutur8"/>
    <tag group="abRange" tag="Personal" name="Personal" abbrev="pers"/>
    <tag group="AbilType" tag="SpellLike" name="Spell-Like Ability" abbrev=" (Sp)"/>
    <tag group="abAction" tag="None" name="No action" abbrev="None"/>
    <tag group="abDuration" tag="Constant" name="Constant" abbrev="cons"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <bootstrap thing="cfgMSRDPsy"></bootstrap>
    <eval phase="Level" priority="5000" index="3"><![CDATA[

      ~ Tag the configurable we bootstrap appropriately.
      ~ The DC of SLAs is based on our INT
      perform hero.childfound[cfgMSRDPsy].assign[StandardDC.aINT]

      ~ The caster level is based on our character's class level
      perform hero.childfound[cfgMSRDPsy].assign[Helper.ClsCastLev]]]>
      <before name="Calc cfgCastLev"/>
      </eval>
    <eval phase="PostLevel" priority="10000" index="4"><![CDATA[

      ~ Mark the spells in our configurable so they have the correct setup
      var selspell as string

      foreach pick in hero from BaseSpell where "CfgSpell.cfgMSRDPsy & Helper.Spell1st"
        ~ Each spell is a SLA which is usable 3 times a day.
        perform eachpick.assign[Helper.SpellLike]
        perform eachpick.assign[Usage.Day]
        eachpick.field[trkMax].value += 3

        ~ They cannot be taken multiple times
        perform eachpick.assign[Helper.NoPrepCnt]

        ~ Note down the id of each spell we take, so that we can amend the
        ~ candidate expression to stop it from being added a second time.
        selspell = splice(selspell, eachpick.tagids[thingid.?], "|")
        nexteach

      ~ Set up the fields of the configurable we bootstrap.
      hero.childfound[cfgMSRDPsy].field[cfgSpell1].text = "Psionics"
      hero.childfound[cfgMSRDPsy].field[cfgAllwSp1].text = "component.BaseSpell & sClass.cHelpPsy & (sLevel.0|sLevel.1|sLevel.2) & !Helper.Obsolete"

      if (length(selspell) <> 0) then
        hero.childfound[cfgMSRDPsy].field[cfgAllwSp1].text &= " & !(" & selspell & ")"
        endif

      ~ Add to the number of choices we get.
      hero.childfound[cfgMSRDPsy].field[cfgMaxSp1].value += 1]]></eval>
    <eval phase="PostLevel" priority="10000" index="7">perform hero.assign[Hero.OccSkUnloc]</eval>
    </thing>
