      if (field[cTotalLev].value >= 6) then
        perform assign[BonusFor.Monk6]
        endif

      if (field[cTotalLev].value >= 10) then
        perform assign[BonusFor.Monk10]
        endif

      if (field[cTotalLev].value >= 14) then
        perform assign[BonusFor.Monk14]
        endif]]></eval>
    <eval phase="First" priority="505" index="2"><![CDATA[
      ~ Monks never have their unarmed strikes reduced to half damage.
      perform hero.childfound[wUnarmed].assign[Helper.NoHalfStr]

      ~ don't do this if the Perfect Self ability is replaced
      doneif (hero.tagis[AbReplace.cMnkPerf] <> 0)

