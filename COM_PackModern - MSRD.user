    <eval phase="Render" priority="20000"><![CDATA[field[abSource].text = "Mutation"]]></eval>
    </thing>
  <thing id="rcMSRDWlCr" name="Wall Crawler" description="You can walk walls and cling to ceilings like a spider. You have tiny barbs on your hands and feet to facilitate climbing, and your fingers and toes secrete a transparent adhesive that lets you cling to smooth surfaces.\n\n{b}MP Cost{/b}: 2.\n\n{b}Benefits{/b}: As long as your hands and feet are uncovered, you can climb perfectly smooth, flat, vertical surfaces. In addition, you gain a +4 mutation bonus on all Climb checks. Wearing gloves or footwear reduces the bonus to +2, and wearing both gloves and footwear negates the bonus entirely.\n\nYou also gain a +4 mutation bonus on Acrobatics checks when moving a cross a slippery surface. Wearing any kind of footwear negates this bonus." compset="RaceCustom" summary="You can walk walls and cling to ceilings like a spider." uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomCost" tag="2"/>
    <eval phase="PreAttr" priority="1000" index="2">#skillbonus[skClimb] += 4</eval>
    <eval phase="Render" priority="20000" index="3"><![CDATA[field[abSource].text = "Mutation"]]></eval>
    <eval phase="Final" priority="20000" index="4"><![CDATA[#situational[hero.child[skAcrobat], " +4 mutation bonus on Acrobatics checks when moving a cross a slippery surface. Wearing any kind of footwear negates this bonus.", field[thingname].text]]]></eval>
    </thing>
  <thing id="rcMSRDWkIs" name="Weak Immune System" description="Your body has difficult preventing infections from entering its system, and thus it is prone to sickness and disease.\n\n{b}MP Value{/b}: 1.\n\n{b}Drawback{/b}: You take a –2 penalty on all Fortitude saves against poison, disease, and radiation sickness.\n\n{b}Special{/b}: You cannot take this drawback if you have the Ultra Immune System feat." compset="RaceCustom" summary="–2 penalty on all Fortitude saves against poison, disease, and radiation sickness." uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="CustomNeg" tag="1"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="SpecType" tag="Weakness"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <eval phase="Final" priority="20000">#situational[hero.child[svFort], &quot; –2 penalty on all Fortitude saves against poison, disease, and radiation sickness.&quot;, field[thingname].text]</eval>
    <eval phase="Render" priority="20000" index="2"><![CDATA[field[abSource].text = "Deformity"]]></eval>
    <pickreq thing="rcMSRDUImS" ispreclude="yes"/>
    <pickreq thing="fMSRDUtImS" ispreclude="yes"/>
    </thing>
  <thing id="rcMSRDWbDt" name="Webbed Digits" description="You grow webbing between your fingers or toes and can move more easily through liquids.\n\n{b}MP Cost{/b}: 1.\n\n{b}Benefit{/b}: You gain a +4 mutation bonus on all Swim checks. Having webbed digits does not interfere with your ability to grasp or manipulate objects." compset="RaceCustom" uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomCost" tag="1"/>
    <eval phase="PreAttr" priority="1000" index="2">#skillbonus[skSwim] += 4</eval>
    <eval phase="Render" priority="20000" index="3"><![CDATA[field[abSource].text = "Mutation"]]></eval>
    </thing>
  <thing id="rcMSRDWing" name="Wings" description="You sprout a pair of birdlike or batlike wings.\n\n{b}MP Cost{/b}: 6.\n\n{b}Benefit{/b}: Your wings grant a natural fly speed of 60 feet (average maneuverability)." compset="RaceCustom" summary="Fly speed of 60 feet (average maneuverability)." uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomCost" tag="6"/>
