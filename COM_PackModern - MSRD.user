      #situational[hero.child[ArmorClass], "+2 AC whenever you have one-quarter, one-half, three-quarters, or nine-tenths cover.", field[thingname].text]

      #situational[hero.child[svRef], "+2 Reflex whenever you have one-quarter, one-half, three-quarters, or nine-tenths cover.", field[thingname].text]]]></eval>
