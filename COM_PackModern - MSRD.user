    <eval phase="Render" priority="20000" index="2"><![CDATA[field[abSource].text = "Mutation"]]></eval>
    </thing>
  <thing id="rcMSRDTnSk" name="Thin Skin" description="You are more susceptible to harm.\n\n{b}MP Value{/b}: 5.\n\n{b}Drawback{/b}: You take 1 additional point of damage each time you are wounded." compset="RaceCustom" summary="1 additional point of damage each time you are wounded." uniqueness="unique">
