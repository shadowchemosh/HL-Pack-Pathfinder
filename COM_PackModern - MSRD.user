  <thing id="ioMSRDPsIp" name="Psi Implant" description="This brain implant stimulates neuron activity in underdeveloped regions of the recipient’s brain, unlocking latent psionic abilities. \n\n{b}Benefit{/b}: Select one 0-level psionic power. The character can manifest this power up to three times in a 24-hour period. There is no power point cost for using this power. For effects that depends on manifester level the character is considered 1st level or his or her manifester level, whichever is higher.\n\n{b}Note{/b}: You also gain access to occult skill unlocks for any skills in which you have ranks. If you have no ranks in the appropriate skill, you can’t use the occult skill unlock, even if that skill can be used untrained.\n\n{b}Type: Internal{/b}. \n{b}Hardness/Hit Points{/b}: –/2.\n\n{b}Construction{/b}\n{b}Requirements{/b}: Craft Cybertech, cybernetics lab; {b}Cost{/b} 31,000 gp" compset="Wondrous" summary="The recipient gains the Wild Psychic Ability feat.">
    <fieldval field="gWeight" value="1"/>
    <fieldval field="gCost" value="3100"/>
    <usesource source="MSRDFutur8"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="SpecGroup" tag="HighTech" name="High-Tech Weapons, Armor, &amp; Equipment" abbrev="High-Tech Weapons, Armor, &amp; Equipment"/>
    <tag group="gType" tag="Wonder"/>
    <tag group="Hero" tag="EqpBrainCy"/>
    <tag group="Helper" tag="NoNightRes"/>
    <tag group="Usage" tag="RoundsAbs"/>
    <tag group="Helper" tag="AlwaysAvl"/>
    <tag group="Implant" tag="3" name="3" abbrev="3"/>
    <bootstrap thing="cfgMSRDPsy"></bootstrap>
    <eval phase="PostAttr" priority="10000"><![CDATA[
      field[livename].text = field[thingname].text & " (Cyberware)"]]></eval>
    <eval phase="PostLevel" priority="10000" index="2">perform hero.assign[Hero.OccSkUnloc]</eval>
    <eval phase="Level" priority="5000" index="3"><![CDATA[

      ~ Tag the configurable we bootstrap appropriately.
      ~ The DC of SLAs is based on our INT
      perform hero.childfound[cfgMSRDPsy].assign[StandardDC.aINT]

      ~ The caster level is based on our character's class level
      perform hero.childfound[cfgMSRDPsy].assign[Helper.ClsCastLev]]]></eval>
    <eval phase="PostLevel" priority="10000" index="4"><![CDATA[

      ~ Mark the spells in our configurable so they have the correct setup
      var selspell as string

      foreach pick in hero from BaseSpell where "CfgSpell.cfgMSRDPsy & Helper.Spell1st"
        ~ Each spell is a SLA which is usable 3 times a day.
        perform eachpick.assign[Helper.SpellLike]
        perform eachpick.assign[Usage.Day]
        eachpick.field[trkMax].value += 3

        ~ They cannot be taken multiple times
        perform eachpick.assign[Helper.NoPrepCnt]

        ~ Note down the id of each spell we take, so that we can amend the
        ~ candidate expression to stop it from being added a second time.
        selspell = splice(selspell, eachpick.tagids[thingid.?], "|")
        nexteach

      ~ Set up the fields of the configurable we bootstrap.
      hero.childfound[cfgMSRDPsy].field[cfgSpell1].text = "Psionics"
      hero.childfound[cfgMSRDPsy].field[cfgAllwSp1].text = "component.BaseSpell & sClass.cHelpPsy & sLevel.0 & !Helper.Obsolete"

      if (length(selspell) <> 0) then
        hero.childfound[cfgMSRDPsy].field[cfgAllwSp1].text &= " & !(" & selspell & ")"
        endif

      ~ Add to the number of choices we get.
      hero.childfound[cfgMSRDPsy].field[cfgMaxSp1].value += 1]]></eval>
    </thing>
