    <usesource source="MSRDFutur7"/>
    <usesource source="MSRDFutur8"/>
    <tag group="fCategory" tag="General"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <eval phase="PostLevel" priority="1000">#skillbonus[skSwim] +=4</eval>
    </thing>
  <thing id="fMSRDSmtPl" name="Smart Plus" description="Advancements in science coupled with your drive to excel enable you to progress faster toward becoming a truly intelligent hero.\n\n{b}Benefit{/b}: You gain two talents from the Smart hero’s talent trees. \n\n{b}Special{/b}: You may select this feat multiple times.  Each time you select this feat, you must choose a different pair of talents." compset="Feat" summary="You gain two talents">
    <usesource source="GRCMP2MSR"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <tag group="fCategory" tag="Special"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <eval phase="PreLevel" priority="10000">hero.childfound[cHelpMSH].field[cGiveSp2nd].value += 2</eval>
    <pickreq thing="cHelpMSH"/>
    </thing>
  <thing id="fMSRDSpace" name="Spacer" description="You have a special affinity for spacecraft and space travel.\n\n{b}Benefit{/b}: You get a +2 bonus on all Computer Use and Pilot skill checks. If you have 10 or more ranks in one of these skills, the bonus increases to +4 for that skill." compset="Feat" summary="+2 bonus on all Computer Use and Pilot skill checks." uniqueness="useronce">
    <fieldval field="shortname" value="Spacer"/>
    <usesource source="GRCMP2MSR"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="fCategory" tag="General"/>
    <eval phase="PostLevel" priority="15000"><![CDATA[      ~ If we're disabled, do nothing
      doneif (tagis[Helper.FtDisable] <> 0)

      #skillbonus[skMSRDCmUs] += 2
      if (#skillranks[skMSRDCmUs] >= 10) then
        #skillbonus[skMSRDCmUs] += 2
        endif

      #skillbonus[skMSRDPilt] += 2
      if (#skillranks[skMSRDPilt] >= 10) then
        #skillbonus[skMSRDPilt] += 2
        endif]]></eval>
    </thing>
  <thing id="fMSRDStrPl" name="Strong Plus" description="Advancements in science coupled with your drive to excel enable you to progress faster toward becoming a truly strong hero.\n\n{b}Benefit{/b}: You gain two talents from the Strong hero’s talent trees. \n\n{b}Special{/b}: You may select this feat multiple times.  Each time you select this feat, you must choose a different pair of talents." compset="Feat" summary="You gain two talents">
    <usesource source="GRCMP2MSR"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <tag group="fCategory" tag="Special"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <eval phase="PreLevel" priority="10000">hero.childfound[cHelpMSg].field[cGiveSp2nd].value += 2</eval>
    <pickreq thing="cHelpMSg"/>
    </thing>
  <thing id="fMSRDTghPl" name="Tough Plus" description="Advancements in science coupled with your drive to excel enable you to progress faster toward becoming a truly tough hero.\n\n{b}Benefit{/b}: You gain two talents from the Tough hero’s talent trees.\n\n{b}Special{/b}: You may select this feat multiple times. Each time you select this feat, you must choose a different pair of talents." compset="Feat" summary="You gain two talents">
    <usesource source="GRCMP2MSR"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <tag group="fCategory" tag="Special"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <eval phase="PreLevel" priority="10000">hero.childfound[cHelpMTg].field[cGiveSp2nd].value += 2</eval>
    <pickreq thing="cHelpMTg"/>
    </thing>
  <thing id="fMSRDXenMd" name="Xenomedic" description="You know how to provide safe medical treatment to alien life forms.\n\n{b}Benefit{/b}: You can, without penalty, use the Heal skill to perform surgery on a living creature regardless of its type.\n\n{b}Normal{/b}: Characters without this feat take a –8 penalty on Heal checks (–4 if they have the Surgery feat) when performing surgery on creatures of a different type.\n\n{b}Special{/b}: This feat cannot be used to heal or repair nonliving or inorganic creatures such as constructs or undead." compset="Feat" summary="Provide safe medical treatment to alien life forms." uniqueness="useronce">
    <usesource source="GRCMP2ScF"/>
    <usesource source="MSRDFutur7"/>
    <usesource source="MSRDFutur8"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="fCategory" tag="General"/>
    <pickreq thing="fMHSurgery"/>
    <exprreq message="Heal 3 ranks"><![CDATA[#skillranks[skHeal] >= 3]]></exprreq>
    <exprreq message="Knowledge (earth and life sciences) 3 ranks"><![CDATA[#skillranks[skMHEandLS] >= 3]]></exprreq>
    </thing>
  <thing id="fMSRDZGTra" name="Zero-G Training" description="You can function normally in low gravity or zero gravity.\n\n{b}Benefit{/b}: You take no penalty on attack rolls in low-gravity or zero-gravity environments. In addition, you do not suffer the debilitating effects of space sickness.\n\n{b}Normal{/b}: Without this feat, you take a –4 penalty on attack rolls while operating in zero-gravity environments, or a –2 penalty on attack rolls while operating in low-gravity environments. In addition, you are subject to the effects of Space Adaptation Syndrome, also known as space sickness." compset="Feat" summary="You can function normally in low gravity or zero gravity." uniqueness="useronce">
    <usesource source="GRCMP2ScF"/>
    <usesource source="MSRDFutur7"/>
    <usesource source="MSRDFutur8"/>
    <tag group="fCategory" tag="General"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <exprreq message="Dexterity 13 required."><![CDATA[#attrvalue[aDEX] >= 13]]></exprreq>
    <exprreq message="Acrobatics 1 Rank required."><![CDATA[#skillranks[skAcrobat] >= 1]]></exprreq>
    </thing>
  <thing id="pMSRDAgChd" name="Aging Effects: Child" description="You are a child and take a -3 to STR and CON and -1 to all other attributes.\n\n{b}Note{/b}: The aging effects here are already stacked - each category adjustment includes the cumulative effects of the younger categories." compset="InPlay" uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="Helper" tag="NoIncr" name="NoIncr" abbrev="NoIncr"/>
    <tag group="Helper" tag="AdjInfo" name="AdjInfo" abbrev="AdjInfo"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <eval phase="First" priority="2000" index="2"><![CDATA[      ~ If we're not enabled, get out now
      doneif (field[pIsOn].value = 0)

     ~ we want to decrease our size by -1
      var sizemod as number
      sizemod = -1
      call SizeChange]]></eval>
    <eval phase="PreAttr" priority="10000"><![CDATA[
      ~ If we're not enabled, get out now
      doneif (field[pIsOn].value = 0)

      ~if either of the older age categories are active, get out
      doneif (hero.childfound[adjAgeOld].field[pIsOn].value <> 0)
      doneif (hero.childfound[adjAgeVen].field[pIsOn].value <> 0)
      doneif (hero.childfound[adjAgeMid].field[pIsOn].value <> 0)

      if (hero.tagmax[Hero.NoAgePen?] < 1) then
        ~-1 to all physical attributes
        hero.child[aSTR].field[aStartMod].value -= 3
        hero.child[aDEX].field[aStartMod].value -= 1
        hero.child[aCON].field[aStartMod].value -= 3
        hero.child[aINT].field[aStartMod].value -= 1
        hero.child[aWIS].field[aStartMod].value -= 1
        hero.child[aCHA].field[aStartMod].value -= 1
        endif]]></eval>
    </thing>
  <thing id="cfgMSRDMut" name="Mutant" compset="Configure" uniqueness="unique">
    <fieldval field="cfgObject1" value="Mutation Points"/>
    <usesource source="GRCMP2MSR"/>
    <tag group="ProductId" tag="HLCommunit"/>
    </thing>
  <thing id="tmMSRDMuta" name="Mutations" description="While a mutation can assume many forms, it typically signifies an abnormal trait or ability not common to the species. Mutations can be pronounced or unnoticeable. They may grant extraordinary abilities or effect subtle to grotesque changes in a creature’s physiology.\n\nMutations may appear only at birth or manifest as consequences of radiation exposure or some horrible physical trauma. Ordinarily, a GM should allow players to select appropriate mutations and drawbacks for their characters using the Mutation Point (MP) system presented below. However, for GM-controlled creatures and supporting characters, the GM can determine mutations randomly. Whenever a creature or supporting character gains a mutation, the GM may roll percentile dice and consult Table: Mutations below, then select an appropriate number of drawbacks with a total MP value equal to or greater than the MP cost of the randomly determined mutation.\n\nAll special qualities granted by mutations are considered extraordinary abilities. Using an extraordinary ability is a free action unless noted otherwise." compset="Template" uniqueness="unique">
    <fieldval field="shortname" value="Mutant"/>
    <arrayval field="tmCRArray" index="0" value="1"/>
    <usesource source="GRCMP2MSR"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="ClVaryName" tag="Prepend"/>
    <tag group="TypeNoEff" tag="tpAberr"/>
    <tag group="TemplHelp" tag="Acquired"/>
    <bootstrap thing="cfgMSRDMut"></bootstrap>
    <prereq message="Corporeal creature required.">
      <validate><![CDATA[
        ~once we've been selected, we're fine
        validif (@ispick <> 0)

        ~we're valid if the incorporeal special isn't present
        validif (#hasability[raIncorpor] = 0)]]></validate>
      </prereq>
    </thing>
  <thing id="rcMSRDAbDc" name="Ability Decay" description="Your body or mind suffers from some marked deformity or deterioration. \n\n{b}MP Value{/b}: 4. \n\n{b}Drawback{/b}: One of your ability scores (your choice) permanently decreases by 2. You cannot apply this drawback to an ability score of 3 or less. \n\n{b}Special{/b}: You may take this drawback multiple times. Its effects stack." compset="RaceCustom" summary="Your body or mind suffers from some marked deformity or deterioration.">
    <fieldval field="usrCandid1" value="component.BaseAttr"/>
    <usesource source="GRCMP2MSR"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="ChooseSrc1" tag="Hero"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomNeg" tag="4"/>
    <tag group="SpecType" tag="Weakness"/>
    <eval phase="PreAttr" priority="5000"><![CDATA[      ~if we've been replaced, get out now
      doneif (tagis[Helper.SpcReplace] <> 0)

      if (field[usrChosen1].ischosen <> 0) then
        field[usrChosen1].chosen.field[aStartMod].value -= 2
        endif]]></eval>
    <eval phase="Render" priority="20000" index="2"><![CDATA[field[abSource].text = "Deformity"]]></eval>
    </thing>
  <thing id="rcMSRDAcSl" name="Acidic Saliva" description="Your saliva can burn other creatures like acid. \n\n{b}MP Cost{/b}: 1. \n\n{b}Benefit{/b}: Your bite attack deals an extra 1d4 points of acid damage with each successful hit. \n\n{b}Special{/b}: You must have a natural bite attack to take this mutation. You cannot gain this mutation if you have the Venomous Bite mutation." compset="RaceCustom" summary="Your saliva can burn other creatures like acid." uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomCost" tag="1"/>
    <eval phase="Final" priority="10000"><![CDATA[
      ~ If we're disabled, just get out now
      doneif (tagis[Helper.SpcDisable] <> 0)

      foreach pick in hero from BaseWep where "thingid.wBite"
        if (length(eachpick.field[wDamExtra].text) = 0) then
          #extradamwithavg[eachpick," plus 1d4 acid",field[thingname].text,"3.5"]
        else
          #extradamwithavg[eachpick," and 1d4 acid",field[thingname].text,"3.5"]
          endif
        nexteach]]></eval>
    <eval phase="Render" priority="20000" index="2"><![CDATA[field[abSource].text = "Mutation"]]></eval>
    <pickreq thing="rcMSRDVnBt" ispreclude="yes"/>
    <pickreq thing="wBite"/>
    </thing>
  <thing id="rcMSRDVnBt" name="Venomous Bite" description="Your natural bite attack injects poison into your victim&apos;s bloodstream. \n\n{b}MP Cost{/b}: 4. \n\n{b}Benefit{/b}: Any creature you successfully hit with your bite attack must make a successful Fortitude save (DC 10 + one-half your level + your Constitution modifier) to negate the effects (initial and secondary damage 1d4 Con). \n\n{b}Special{/b}: You must have a natural bite attack to gain the benefit of this mutation. You cannot take this mutation if you have the Acidic Saliva mutation." compset="RaceCustom" summary="Your natural bite attack injects poison into your victim&apos;s bloodstream." uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="StandardDC" tag="aCON"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomCost" tag="4"/>
    <pickreq thing="wBite"/>
    <pickreq thing="rcMSRDAcSl" ispreclude="yes"/>
    </thing>
  <thing id="rcMSRDAdJt" name="Adrenaline Jolt" description="You can flood your bloodstream with extreme amounts of adrenaline to temporarily boost your Strength and Dexterity. \n\n{b}MP Cost{/b}: 3. \n\n{b}Benefit{/b}: Once per day, as a free action, you can temporarily increase your Strength and Dexterity by 4 points. The increase lasts for a number of rounds equal to 3 + your Constitution modifier." compset="RaceCustom" summary="Temporarily boost your Strength and Dexterity." uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="User" tag="Tracker"/>
    <tag group="Usage" tag="DayRound"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="CustomCost" tag="3"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="User" tag="Activation"/>
    <eval phase="PostAttr" priority="10000">~uses per day = 3 + CON mod
      field[trkMax].value += 3 + hero.child[aCON].field[aModBonus].value</eval>
    <eval phase="Level" priority="1000" index="2"><![CDATA[
      if (field[abilActive].value <> 0) then

        #applybonus[ModCirc,hero.child[aSTR], 5]
        #applybonus[ModCirc,hero.child[aDEX], 5]
        endif]]></eval>
    <eval phase="Render" priority="20000" index="3"><![CDATA[field[abSource].text = "Mutation"]]></eval>
    </thing>
  <thing id="rcMSRDBldH" name="Blood Hunger" description="You crave the taste of blood. Moreover, you need to drink blood to survive. \n\n{b}MP Cost{/b}: 1. \n\n{b}Drawback{/b}: You must drain a pint of blood from a living creature once every 24 hours. Doing so is an attack action, and you can only drain blood from a willing, helpless, or dying (but not dead) creature. The bitten creature takes normal damage from the bite attack plus an extra 1d6 points of damage from the blood loss. \n\nIf you go 24 hours without consuming blood, you take 1d4 points of Constitution damage. Drinking a pint of blood cures the ability damage caused by blood deprivation in 1d6 rounds. Ability damage caused by blood deprivation cannot be restored through natural healing. \n\nSpecial: You must have a natural bite attack to take this drawback." compset="RaceCustom" summary="You crave the taste of blood, you need to drink blood to survive." uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomNeg" tag="1"/>
    <tag group="SpecType" tag="Weakness"/>
    <eval phase="Final" priority="10000"><![CDATA[
      ~ If we're disabled, just get out now
      doneif (tagis[Helper.SpcDisable] <> 0)

      foreach pick in hero from BaseWep where "thingid.wBite"
        if (length(eachpick.field[wDamExtra].text) = 0) then
          #extradamwithavg[eachpick," plus 1d6 blood loss",field[thingname].text,"3.5"]
        else
          #extradamwithavg[eachpick," and 1d6 blood loss",field[thingname].text,"3.5"]
          endif
        nexteach]]></eval>
    <eval phase="Render" priority="20000" index="2"><![CDATA[field[abSource].text = "Deformity"]]></eval>
    <pickreq thing="wBite"/>
    </thing>
  <thing id="rcMSRDBrBn" name="Brittle Bones" description="Your bones weaken such that you can no longer withstand hard or sudden impacts. \n\n{b}MP Value{/b}: 4. \n\n{b}Drawbacks{/b}: Your Hit Pont Total decreases by 5. In addition, you take an additional 1d6 points of damage from a fall. \n\n{b}Special{/b}: You cannot take this drawback if you have the Skeletal Reinforcement mutation." compset="RaceCustom" summary="Your Hit Pont Total decreases by 5." uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="CustomNeg" tag="4"/>
    <tag group="SpecType" tag="Weakness"/>
    <eval phase="PostLevel" priority="5000">~ add that bonus to our hit points
        container.child[Totals].field[tHP].value -= 5</eval>
    <eval phase="Render" priority="20000" index="2"><![CDATA[field[abSource].text = "Deformity"]]></eval>
    <pickreq thing="rcMSRDSkRn" ispreclude="yes"/>
    </thing>
  <thing id="rcMSRDSkRn" name="Skeletal Reinforcement" description="Your bones become more resilient, allowing you withstand greater amounts of punishment. \n\n{b}MP Cost{/b}: 5. \n\n{b}Benefits{/b}: Your Hit Points increases by +5. In addition, the damage you take from a fall is reduced by one die. \n\n{b}Special{/b}: You cannot take this mutation if you have the Brittle Bones drawback." compset="RaceCustom" summary="+5 HP/ damage you take from a fall is reduced by one die." uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomCost" tag="5"/>
    <eval phase="PostLevel" priority="5000">~ add that bonus to our hit points
        container.child[Totals].field[tHP].value += 5</eval>
    <eval phase="Render" priority="20000" index="2"><![CDATA[field[abSource].text = "Mutation"]]></eval>
    <pickreq thing="rcMSRDBrBn" ispreclude="yes"/>
    </thing>
  <thing id="rcMSRDClaw" name="Claws" description="Your hands mutate into sharp claws. \n\n{b}MP Cost{/b}: 1. \n\n{b}Benefit{/b}: You gain a single claw attack that deals slashing damage. If you get multiple attacks in a round, you can strike with multiple claws. Your claws are treated as natural weapons and do not provoke attacks of opportunity. \n\nHaving claws does not adversely affect your manual dexterity." compset="RaceCustom" summary="Your hands mutate into sharp claws." uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomCost" tag="1"/>
    <bootstrap thing="wClaw">
      <autotag group="wMain" tag="1d4"/>
      <autotag group="Helper" tag="NatSizeDmg"/>
      <autotag group="SpecSource" tag="tmMSRDMuta"/>
      </bootstrap>
    <eval phase="PostAttr" priority="10000"><![CDATA[        perform hero.findchild[BaseNatWep, "IsWeapon.wClaw & SpecSource.tmMSRDMuta"].assignstr["Value." & herofield[tNumHands].value]]]></eval>
    <eval phase="Render" priority="20000" index="2"><![CDATA[field[abSource].text = "Mutation"]]></eval>
    </thing>
  <thing id="rcMSRDCbFr" name="Combat Fear" description="Due to a chemical imbalance in your brain, you are gripped by an inexplicable fear whenever you face a dangerous or frightening situation. \n\n{b}MP Value{/b}: 4. \n\n{b}Drawback{/b}: After initiative is rolled but before you take your first action in combat, make a Will saving throw (DC 15). If you fail the Will save, you are shaken for the rest of the encounter, taking a -2 penalty on attack rolls, saving throws, and skill checks. If the save succeeds, you overcome your moment of fear and negate the ill effects." compset="RaceCustom" summary="Will save (DC15) or shaken for the rest of the encounter." uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="SpecType" tag="Weakness"/>
    <tag group="CustomNeg" tag="4"/>
    <eval phase="Render" priority="20000"><![CDATA[field[abSource].text = "Deformity"]]></eval>
    </thing>
  <thing id="rcMSRDCyDp" name="Cybernetic Dependency" description="Radiation exposure causes your body tissues to degenerate. You rely on a cybernetic implant to repair damage to your body. \n\n{b}MP Value{/b}: 6. \n\n{b}Drawback{/b}: You need a body repair weave (see Cybernetics) to heal naturally. For you, the body repair weave does not confer its usual benefit (improved natural healing). If the implant is destroyed, you cannot heal damage naturally until it is replaced or repaired. \n\n{b}Special{/b}: This drawback is available only in Progress Level 7 or higher campaigns." compset="RaceCustom" summary="You need a cybernetic Body Repair Weave to heal naturally." uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="CustomNeg" tag="6"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="SpecType" tag="Weakness"/>
    <tag group="AbilType" tag="Extra"/>
    <eval phase="Render" priority="20000"><![CDATA[field[abSource].text = "Deformity"]]></eval>
    <pickreq thing="ioMSRDBdRW"/>
    </thing>
  <thing id="rcMSRDDkvs" name="Darkvision" description="You gain darkvision. \n\n{b}MP Cost{/b}: 3. \n\n{b}Benefit{/b}: You can see in total darkness out to a range of 60 feet. Darkvision is black and white only but is otherwise like normal sight." compset="RaceCustom" summary="You can see in total darkness out to a range of 60 feet." uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomCost" tag="3"/>
    <tag group="Helper" tag="SpecUp"/>
    <bootstrap thing="raDarkVis">
      <autotag group="Value" tag="60"/>
      <assignval field="abSource" value="Mutation"/>
      </bootstrap>
    <eval phase="Render" priority="20000"><![CDATA[field[abSource].text = "Mutation"]]></eval>
    </thing>
  <thing id="rcMSRDEchl" name="Echolocator" description="You develop the ability to absorb sound waves and translate them into mental images that accurately portray the surrounding environment. This ability is similar to a bat&apos;s ability to operate and hunt in total darkness via echolocation. \n\n{b}MP Cost{/b}: 5. \n\n{b}Benefit{/b}: You gain the blindsight ability out to a range of 60 feet." compset="RaceCustom" summary="You develop the ability to absorb sound waves and translate them." uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomCost" tag="5"/>
    <tag group="Helper" tag="SpecUp"/>
    <bootstrap thing="rBlindSi">
      <autotag group="Value" tag="60"/>
      <assignval field="abSource" value="Mutation"/>
      </bootstrap>
    <eval phase="Render" priority="20000"><![CDATA[field[abSource].text = "Mutation"]]></eval>
    </thing>
  <thing id="rcMSRDElas" name="Elasticity" description="You can bend and twist your body in unnatural ways and squeeze through very tight spaces. \n\n{b}MP Cost{/b}: 4. \n\n{b}Benefit{/b}: You gain a +10 mutation bonus on Escape Artist checks. Moreover, you can squeeze through an opening or passage one-tenth as wide and tall as your height, in inches. A creature using this mutation to move through a tight space moves at one-quarter normal speed. ." compset="RaceCustom" summary="You can squeeze through very tight spaces." uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomCost" tag="4"/>
    <eval phase="PreLevel" priority="10000">#skillbonus[skEscape] += 10</eval>
    <eval phase="Render" priority="20000" index="2"><![CDATA[field[abSource].text = "Mutation"]]></eval>
    </thing>
  <thing id="rcMSRDEAAd" name="Energy Absorption (Acid)" description="You can absorb impressive amounts of harmful energy. \n\n{b}MP Cost{/b}: 4. \n\n{b}Benefit{/b}: You gain resistance 20 to one type of energy, chosen from the following list: acid, cold, electricity, fire, or sonic/concussion." compset="RaceCustom" uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
