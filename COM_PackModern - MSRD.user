    <eval phase="First" priority="1000">hero.child[svWill].field[Bonus].value -= 2</eval>
    <pickreq thing="fIronWill" ispreclude="yes"/>
    </thing>
  <thing id="rcMSRDNeDp" name="Neutrad Dependency" description="You rely on a medical compound called neutrad to survive. \n\n{b}MP Value{/b}: 6. \n\n{b}Drawback{/b}: If you go 24 hours without taking a dose of neutrad you must make a successful Fortitude save (DC 15) or take 1 point of Strength damage and 1 point of Constitution damage. Taking a dose of neutrad cures the ability damage caused by neutrad deprivation in 1d6 rounds. Ability damage caused by neutrad deprivation cannot be restored through natural healing. \n\n{b}Special{/b}: This drawback can be taken only in Progress Level 6 or higher campaigns (when neutrad is available)." compset="RaceCustom" summary="You rely on a medical compound called neutrad to survive." uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomNeg" tag="6"/>
    <tag group="SpecType" tag="Weakness"/>
    <eval phase="Render" priority="20000"><![CDATA[field[abSource].text = "Deformity"]]></eval>
    </thing>
  <thing id="rcMSRDPhAt" name="Pheromone Attraction" description="You can regulate the production of pheromones in your body and release them at will, altering the moods of other nearby creatures. \n\n{b}MP Cost{/b}: 6. \n\n{b}Benefits{/b}: You gain a +4 mutation bonus on all Bluff, Diplomacy, Handle Animal, and Intimidate checks made against creatures within 30 feet of you. \n\n{b}Special{/b}: You cannot take this mutation if you have the Pheromone Repulsion drawback." compset="RaceCustom" summary="+4 bonus Bluff, Diplomacy, Handle Animal, and Intimidate." uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="CustomCost" tag="6"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="User" tag="Activation"/>
    <eval phase="Level" priority="1000" index="2"><![CDATA[
      if (field[abilActive].value <> 0) then

        #applybonus[ModCirc,hero.child[skBluff], 4]
        #applybonus[ModCirc,hero.child[skDiplo], 4]
        #applybonus[ModCirc,hero.child[skHandleAn], 4]
        #applybonus[ModCirc,hero.child[skIntim], 4]
        endif]]></eval>
    <eval phase="Render" priority="20000" index="3"><![CDATA[field[abSource].text = "Mutation"]]></eval>
    <pickreq thing="rcMSRDPhRp" ispreclude="yes"/>
    </thing>
  <thing id="rcMSRDPhRp" name="Pheromone Repulsion" description="You release pheromones that other creatures find repulsive. \n\n{b}MP Value{/b}: 1. \n\n{b}Drawbacks{/b}: You take a -4 penalty on all Diplomacy and Handle Animal checks made against creatures within 30 feet of you. \n\n{b}Special{/b}: You cannot take this drawback if you have the Pheromone Attraction mutation." compset="RaceCustom" uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="CustomNeg" tag="1"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="User" tag="Activation"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="SpecType" tag="Weakness"/>
    <eval phase="Render" priority="20000" index="3"><![CDATA[field[abSource].text = "Deformity"]]></eval>
    <eval phase="Level" priority="1000" index="2"><![CDATA[
      if (field[abilActive].value <> 0) then

        #applybonus[ModCirc,hero.child[skDiplo], -4]
        #applybonus[ModCirc,hero.child[skHandleAn], -4]
        endif]]></eval>
    <pickreq thing="rcMSRDPhAt" ispreclude="yes"/>
    </thing>
  <thing id="rcMSRDPsBd" name="Poisonous Blood" description="Your blood is poisonous to your own body. You must take regular doses of antitox to resist the poison’s effects.\n\n{b}MP Value{/b}: 6.\n\n{b}Drawback{/b}: If you go 24 hours without taking a dose of antitox you must make a successful Fortitude save (DC 15) or take 1d4 points of Constitution damage. Taking a dose of antitox cures the ability damage caused by antitox deprivation in 1d6 rounds. Ability damage caused by antitox deprivation cannot be restored through natural healing.\n\n{b}Special{/b}: This drawback can be taken only in Progress Level 6 or higher campaigns (when antitox is available)." compset="RaceCustom" summary="Your blood is poisonous to your own body." uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="StandardDC" tag="aCON"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomNeg" tag="6"/>
    <tag group="SpecType" tag="Weakness"/>
    <eval phase="Render" priority="20000"><![CDATA[field[abSource].text = "Deformity"]]></eval>
    </thing>
  <thing id="rcMSRDPrTl" name="Prehensile Tail" description="You grow a tail that can grasp and hold objects.\n\n{b}MP Cost{/b}: 4.\n\n{b}Benefits{/b}: A prehensile tail grants a +2 mutation bonus on Acrobatics checks. It can also grasp and manipulate a simple object up to one size category smaller than the creature’s size category. A prehensile tail cannot be used to operate a piece of equipment that requires opposable digits or fine motor control (such as a cell phone).\n\nA creature can “hang” from its prehensile tail indefinitely by wrapping it around a larger object, thereby freeing up its other limbs. The prehensile tail isn’t dexterous or strong enough to fire ranged weapons or make melee attacks, however.\n\n{b}Special{/b}: A creature that already has a tail cannot gain this mutation." compset="RaceCustom" summary="You grow a tail that can grasp and hold objects." uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomCost" tag="4"/>
    <eval phase="PreAttr" priority="1000" index="2">#skillbonus[skAcrobat] += 2</eval>
    <eval phase="Render" priority="20000" index="3"><![CDATA[field[abSource].text = "Mutation"]]></eval>
    <pickreq thing="rcMSRDTail" ispreclude="yes"/>
    </thing>
  <thing id="rcMSRDTail" name="Tail" description="You grow a thick tail. The tail may be fur-covered, slender and whiplike like a rat’s, or scaly like a lizard’s. Although the tail improves your balance and can serve as a weapon, it cannot be used for gripping objects.\n\n{b}MP Cost{/b}: 1.\n\n{b}Benefits{/b}: The tail provides a +2 mutation bonus on all Acrobatics checks. In addition, you gain a single tail slam attack that deals bludgeoning damage dependent on your size: Small 1d4, Medium-size 1d6, Large 1d8. If you get multiple attacks in a round, you can strike multiple times with your tail. Your tail is treated as a natural weapon and does not provoke attacks of opportunity.\n\n{b}Special{/b}: A creature that already has a tail or the Prehensile Tail mutation cannot gain this mutation. This mutation can be used in conjunction with the Stinger mutation." compset="RaceCustom" summary="You grow a thick tail." uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomCost" tag="1"/>
    <bootstrap thing="wSlam">
      <autotag group="wMain" tag="1d6_5"/>
      <autotag group="Value" tag="1"/>
      </bootstrap>
    <eval phase="PreAttr" priority="1000" index="2">#skillbonus[skAcrobat] += 2</eval>
    <eval phase="Render" priority="20000" index="3"><![CDATA[field[abSource].text = "Mutation"]]></eval>
    <pickreq thing="rcMSRDPrTl" ispreclude="yes"/>
    </thing>
  <thing id="rcMSRDPkPr" name="Prickly Pear" description="Bony spurs or chitinous spikes protrude from your joints, giving you a jagged profile and making you dangerous to grapple.\n\n{b}MP Cost{/b}: 4.\n\n{b}Benefits{/b}: You deal 1d6 points of piercing damage to any creature you are grappling or any creature that grapples you. Furthermore, you deal 1d6 points of piercing damage per round to any creature that swallows you using the swallow whole abilit" compset="RaceCustom" summary="Bony spurs or chitinous spikes protrude from your joints." uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomCost" tag="4"/>
    <tag group="Helper" tag="SpecUp"/>
    <bootstrap thing="wManSpike">
      <autotag group="wMain" tag="1d6"/>
      <autotag group="Value" tag="1"/>
      </bootstrap>
    <eval phase="Render" priority="20000"><![CDATA[field[abSource].text = "Mutation"]]></eval>
    </thing>
  <thing id="rcMSRDRdac" name="Radioactive" description="You are immune to some radiation and can emit bursts of harmful radiation from your body.\n\n{b}MP Cost{/b}: 5.\n\n{b}Benefits{/b}: You are immune to mild, low, and moderate degrees of radiation exposure. In addition, your body acts as a radiation battery, storing the energy for later use. Once per day as a free action, you may release a 60-foot-radius burst of radiation centered on you. All creatures within the burst radius are exposed to a moderate degree of radiation (Fortitude DC 18, incubation period 3d6 hours, initial and secondary damage 1d6–1 Con)." compset="RaceCustom" summary="You are immune to some radiation" uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomCost" tag="5"/>
    <bootstrap thing="xImmRadiat"></bootstrap>
    <eval phase="Render" priority="20000"><![CDATA[field[abSource].text = "Mutation"]]></eval>
    </thing>
  <thing id="rcMSRDRdAg" name="Rapid Aging" description="You age at an increased rate due to the strain your mutations place on your cells. You appear many years older than your actual age.\n\n{b}MP Value{/b}: 2.\n\n{b}Drawback{/b}: Double your actual age to get your effective age. Your effective age determines your ability score penalties. You do not gain any of the benefits of aging (increased mental ability scores)." compset="RaceCustom" summary="You appear many years older than your actual age." uniqueness="unique">
    <arrayval field="usrArray" index="0" value="middle-aged"/>
    <arrayval field="usrArray" index="1" value="old"/>
    <arrayval field="usrArray" index="2" value="venerable"/>
    <usesource source="GRCMP2MSR"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="SpecType" tag="Weakness"/>
    <tag group="abAction" tag="Constant"/>
    <tag group="abRange" tag="Personal"/>
    <tag group="abDuration" tag="Permanent"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="CustomNeg" tag="2"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <eval phase="Render" priority="20000"><![CDATA[field[abSource].text = "Deformity"]]></eval>
    <eval phase="PreAttr" priority="10000" index="2">if (field[usrIndex].value = 0) then
        ~-1 to all physical attributes
        hero.child[aSTR].field[aStartMod].value -= 1
        hero.child[aDEX].field[aStartMod].value -= 1
        hero.child[aCON].field[aStartMod].value -= 1
        endif</eval>
    <eval phase="PreAttr" priority="10000" index="3">if (field[usrIndex].value = 1) then
        ~-3 to all physical attributes
        hero.child[aSTR].field[aStartMod].value -= 3
        hero.child[aDEX].field[aStartMod].value -= 3
        hero.child[aCON].field[aStartMod].value -= 3
        endif</eval>
    <eval phase="PreAttr" priority="10000" index="4">if (field[usrIndex].value = 2) then
        ~-6 to all physical attributes
        hero.child[aSTR].field[aStartMod].value -= 6
        hero.child[aDEX].field[aStartMod].value -= 6
        hero.child[aCON].field[aStartMod].value -= 6
        endif</eval>
    </thing>
  <thing id="rcMSRDRdSp" name="Reduced Speed" description="You are unable to move as quickly as normal due to various mutations and deformities.\n\n{b}MP Value{/b}: 3.\n\n{b}Drawback{/b}: Reduce your speed by 5 feet. This speed decrease also applies to any natural burrow, climb, fly, or swim speed you might have. \n\n{b}Special{/b}: You may take this drawback multiple times. Its effects stack." compset="RaceCustom">
