  <thing id="rcMSRDEnFm" name="Enlarged Form" description="You grow, becoming a freakishly large specimen of your kind. \n\n{b}MP Cost{/b}: 6. \n\n{b}Benefits{/b}: You become as large as your size category allows (8 feet tall for Medium-size characters, 4 feet tall for Small characters). However, you function in many ways as if you were one size category larger. Whenever you are subject to a size modifier or special size modifier for an opposed check (such as during grapple checks, bull rush attempts, and trip attempts), you are treated as one size larger when it is advantageous to you. You are also considered to be one size larger when determining whether special attacks based on size (such as improved grab or swallow whole) can affect you. You can use weapons designed for a creature one size larger without penalty (for example, a Medium-size character with this mutation can wield a Large weapon one-handed). \n\nThis mutation does not change your face or reach." compset="RaceCustom" summary="You grow, becoming a freakishly large specimen of your kind." uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomCost" tag="6"/>
    <bootstrap thing="xPowBuild"></bootstrap>
    <eval phase="Render" priority="20000"><![CDATA[field[abSource].text = "Mutation"]]></eval>
    <eval phase="First" priority="10000" index="2">foreach pick in hero from BaseWep
        perform eachpick.tagreplace[wClass.TwoHanded,wClass.OneHanded]
        nexteach</eval>
    </thing>
  <thing id="rcMSRDExSk" name="Exoskeleton" description="A thick exoskeleton forms over your skin. The exoskeleton usually consists of interlocking plates of bone or chitin covering 90% or more of your body. \n\n{b}MP Cost{/b}: 5. \n\n{b}Benefit{/b}: You gain a +3 natural armor bonus to AC, or your existing natural armor bonus improves by 3. \n\n{b}Special{/b}: A creature with fur, scales, or the Scaly Armor mutations cannot gain this mutation." compset="RaceCustom" summary="A thick exoskeleton forms over your skin." uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomCost" tag="5"/>
    <eval phase="First" priority="10000">hero.child[ArmorClass].field[tACNatural].value +=3</eval>
    <eval phase="Render" priority="20000" index="2"><![CDATA[field[abSource].text = "Mutation"]]></eval>
    <pickreq thing="rcMSRDScSk" ispreclude="yes"/>
    <pickreq thing="rcMSRDTkFC" ispreclude="yes"/>
    <pickreq thing="rcMSRDTnFC" ispreclude="yes"/>
    <pickreq thing="rcMSRDScAr" ispreclude="yes"/>
    </thing>
  <thing id="rcMSRDTkFC" name="Thick Fur Coat" description="You grow a thick, protective layer of fur over your body. \n\n{b}MP Cost{/b}: 1. \n\n{b}Benefits{/b}: You gain a +4 mutation bonus on Fortitude saves against extreme cold temperatures. \n\n{b}Special{/b}: A creature with fur, scales, or chitin cannot gain this mutation." compset="RaceCustom" summary="+4 Fortitude saves against extreme cold temperatures." uniqueness="unique">
    <fieldval field="abValue" value="4"/>
    <usesource source="GRCMP2MSR"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomCost" tag="1"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <eval phase="PostLevel" priority="10000" index="2"><![CDATA[      #situational[hero.child[svFort],signed(field[abValue].value) & " bonus on Fortitude saves against extreme cold temperatures. ",field[thingname].text]]]></eval>
    <eval phase="Render" priority="20000"><![CDATA[field[abSource].text = "Mutation"]]></eval>
