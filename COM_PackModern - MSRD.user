    <tag group="Helper" tag="NotMagicPw" name="NotMagicPw" abbrev="NotMagicPw"/>
    <tag group="PowerType" tag="Armor"/>
    <tag group="ProductId" tag="HLCommunit" name="Hero Lab Community" abbrev="Hero Lab Community"/>
    <tag group="SpecGroup" tag="FireAdvan" name="Firearm, Advanced" abbrev="Firearm, Advanced"/>
    <tag group="ChooseSrc2" tag="Hero"/>
    <tag group="User" tag="Activation" name="Activation" abbrev="Activation"/>
    <tag group="User" tag="NoActSign" name="NoActSign" abbrev="NoActSign"/>
    <tag group="Helper" tag="Shadow"/>
    <tag group="fShowWhat" tag="Weapons"/>
    <tag group="abCategory" tag="ArmorModif"/>
    <eval phase="Final" priority="10000" index="5"><![CDATA[doneif (field[abilActive].value = 0)
if (field[usrChosen1].ischosen <> 0) then
      if (parent.tagis[component.BaseWep] <> 0) then
        #extradamage[parent," or " & field[iCustSpec].text & field[usrChosen1].chosen.field[name].text, field[thingname].text]
      else
        foreach pick in hero from BaseWep where container.child[gCustMagic].field[gWeapExpr].text
          #extradamage[eachpick," or " & field[iCustSpec].text & field[usrChosen1].chosen.field[name].text, field[thingname].text]
          nexteach
        endif
endif]]></eval>
    <eval phase="First" priority="200" index="6"><![CDATA[if (field[abilActive].value <> 0) then

perform field[usrChosen1].chosen.setfocus

perform focus.assign[Helper.NoHandWeap]

endif]]></eval>
