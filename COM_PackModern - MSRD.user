  <thing id="cMSRDImAdA" name="Improved Aid Another" description="The Dedicated hero&apos;s bonus on attempts to aid another increases by +1 on a successful aid another check. This talent can be selected multiple times, each time increasing the bonus by +1." compset="CustomSpec" summary="Your bonus on attempts to aid another increases by +1">
    <usesource source="GRCMP2tMP3"/>
    <tag group="Helper" tag="Secondary"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="SpecSource" tag="cHelpMSD"/>
    <tag group="SpecSource" tag="arMSRDDedO"/>
    <eval phase="PostLevel" priority="10000"><![CDATA[~ see if a quickfind has been created by another copy of this ability

perform quickfind.setfocus

if (state.isfocus = 0) then
      perform quickadd
      perform assign[Helper.FirstCopy]
      field[abValue].value = 1

      field[livename].text = field[thingname].text & " (" & signed(field[abValue].value) & " Bonus to Aid Another)"

else
      perform focus.redirect
      perform assign[Helper.SpecUp]

      focus.field[abValue].value += 1

      hero.child[cMSRDImAdA].field[livename].text = field[thingname].text & " (" & signed(focus.field[abValue].value) & " Bonus to Aid Another)"
      hero.child[cMSRDImAdA].field[abSumm].text = "Your bonus on attempts to aid another increases by " & signed(focus.field[abValue].value)

      field[livename].text = field[thingname].text & " Specialization"
      field[abSumm].text = "Increases Improved Aid Another total by 1"
endif]]></eval>
    <pickreq thing="cMPSGGEmpa"/>
    </thing>
  <thing id="cMSRDIntui" name="Intuition" description="The Dedicated hero has an innate ability to sense trouble in the air. They can make a Will saving throw (DC 15) and on a successful save, they gets a hunch that everything is all right, or the get a bad feeling about a specific situation.\n\nThis is based on the host&apos;s best guess relating to the circumstances.\n\nYou can use this ability a number of times per day equal to 3 + your Wisdom modifier." compset="CustomSpec" summary="You can sense trouble in the air after a DC15 Will save" uniqueness="useronce">
    <usesource source="GRCMP2tMP3"/>
