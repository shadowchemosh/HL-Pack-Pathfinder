    <pickreq thing="rcMSRDTkFC" ispreclude="yes"/>
    <pickreq thing="rcMSRDTnFC" ispreclude="yes"/>
    <pickreq thing="rcMSRDExSk" ispreclude="yes"/>
    </thing>
  <thing id="rcMSRDExAm" name="Extra Arms" description="You grow an additional pair of arms. The extra arms look and behave exactly like your other arms. \n\n{b}MP Cost{/b}: 6. \n\n{b}Benefit{/b}: As a creature with more than two arms, you gain a +4 mutation bonus on Climb checks and CMD/CMB. For the purposes of combat, both extra arms are treated as &quot;off hands&quot; (that is, you still have only one primary hand). \n\n{b}Special{/b}: If you have three or more natural claw attacks, you meet the prerequisites for the Multiattack feat. This mutation does not give you Multiattack as a bonus feat, however." compset="RaceCustom" summary="You grow an additional pair of arms." uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomCost" tag="6"/>
    <eval phase="PreAttr" priority="1000" index="2">#skillbonus[skClimb] += 4</eval>
    <eval phase="PostLevel" priority="10000" index="3">herofield[tNumHands].value += 2</eval>
    <eval phase="PostLevel" priority="1000">hero.child[Maneuver].field[tCMB].value += 4
      hero.child[Maneuver].field[tCMB].value += 4</eval>
    <eval phase="Render" priority="20000" index="4"><![CDATA[field[abSource].text = "Mutation"]]></eval>
    </thing>
  <thing id="rcMSRDExDg" name="Extra Digits" description="You gain an extra digit on each hand and foot. The extra fingers or toes, although fully functional, do not confer any special benefit, nor do they hinder you in any way. \n\n{b}MP Cost{/b}: 0. \n\n{b}Benefit{/b}: None." compset="RaceCustom" summary="You gain an extra digit on each hand and foot." uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomCost" tag="0"/>
    <eval phase="Render" priority="20000"><![CDATA[field[abSource].text = "Mutation"]]></eval>
    </thing>
  <thing id="rcMSRDFngs" name="Fangs" description="Your teeth mutate into vicious fangs. \n\n{b}MP Cost{/b}: 1. \n\n{b}Benefit{/b}: You gain a vicious bite attack that deals piercing damage. If you get multiple attacks in a round, you can bite multiple times. Your bite is treated as a natural weapon and does not provoke attacks of opportunity. \n\n{b}Special{/b}: This mutation can be used in conjunction with the Acidic Saliva or Venomous Bite mutation." compset="RaceCustom" summary="Your teeth mutate into vicious fangs." uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomCost" tag="1"/>
    <bootstrap thing="wBite">
      <autotag group="wMain" tag="1d6_5"/>
      <autotag group="Value" tag="2"/>
      </bootstrap>
    <eval phase="Render" priority="20000"><![CDATA[field[abSource].text = "Mutation"]]></eval>
    </thing>
  <thing id="rcMSRDFsSr" name="Festering Sores" description="Your skin is covered with painful, festering sores. \n\n{b}MP Value{/b}: 2. \n\n{b}Drawback{/b}: The festering sores are more aggravating than harmful, but they make it especially hard to wear armor. When you wear any type of armor increase its armor penalty by +4." compset="RaceCustom" summary="Increase its armor penalty by +4." uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomNeg" tag="2"/>
    <tag group="SpecType" tag="Weakness"/>
    <eval phase="Render" priority="20000" index="2"><![CDATA[field[abSource].text = "Deformity"]]></eval>
    <eval phase="PostLevel" priority="3000"><![CDATA[
      foreach pick in hero from BaseArmor where "EquipType.Armor"
        eachpick.field[arArmorChk].value -= 4
        nexteach]]></eval>
    </thing>
  <thing id="rcMSRDFins" name="Fins" description="Your body sprouts fishlike fins. A fin might begin on the top of your head and go all the way down your spine. Others might appear on your forearms or calves, or they might sprout from your shoulders or ears. You gain a swim speed equal to its base speed. \n\n{b}MP Cost{/b}: 1. \n\n{b}Benefit{/b}: You gain a swim speed equal to its base speed." compset="RaceCustom" summary="Your body sprouts fishlike fins." uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomCost" tag="1"/>
    <tag group="Helper" tag="SpecUp"/>
    <eval phase="Final" index="14000">~ Set our swim speed
      hero.child[xSwim].field[abValue].value = maximum(hero.child[xSwim].field[abValue].value, hero.child[Speed].field[tSpeed].value)</eval>
    <eval phase="Render" priority="20000" index="14001"><![CDATA[field[abSource].text = "Mutation"]]></eval>
    </thing>
  <thing id="rcMSRDFcBr" name="Force Barrier" description="You can generate an electromagnetic force field around yourself to deflect or absorb incoming attacks. \n\n{b}MP Cost{/b}: 3. \n\n{b}Benefit{/b}: As a free action, you can generate an invisible electromagnetic barrier around yourself that provides damage reduction 3/- against incoming weapon attacks and force effects. You can create the force field a number of rounds equal to your current Constitution modifier +1 (minimum 1 round) per day. \n\n{b}Special{/b}: Portable electronic devices such as cell phones, sensor comps, and headsets temporarily cease to function if held or worn by you while the electromagnetic force barrier is in effect." compset="RaceCustom" summary="You can generate an electromagnetic force field around yourself" uniqueness="unique">
    <fieldval field="abValue" value="3"/>
    <fieldval field="actName" value="Electromagnetic barrier"/>
    <usesource source="GRCMP2MSR"/>
    <tag group="AbilType" tag="PsiLike"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="CustomCost" tag="3"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="User" tag="Activation"/>
    <tag group="abAction" tag="Free"/>
    <tag group="abRange" tag="Personal"/>
    <tag group="abDuration" tag="Day"/>
    <tag group="Usage" tag="Day"/>
    <tag group="User" tag="Tracker"/>
    <eval phase="PostAttr" priority="10000">~uses per day = 1 + CON mod
      field[trkMax].value += 1 + hero.child[aCON].field[aModBonus].value</eval>
    <eval phase="PostAttr" priority="1000" index="2"><![CDATA[
      if (field[abilActive].value <> 0) then

  #applydr[xDamRd, field[abValue].value]
        endif]]></eval>
    <eval phase="Render" priority="20000" index="3"><![CDATA[field[abSource].text = "Mutation"]]></eval>
    </thing>
  <thing id="rcMSRDFkTg" name="Forked Tongue" description="You gain a forked tongue like that of a snake. Your new tongue might be a different color and longer than your old one. \n\n{b}MP Cost{/b}: 0. \n\n{b}Benefit{/b}: None." compset="RaceCustom" uniqueness="unique">
