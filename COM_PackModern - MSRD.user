    <eval phase="Final" priority="20000">#situational[hero.child[svFort], &quot; –2 penalty on all Fortitude saves against poison, disease, and radiation sickness.&quot;, field[thingname].text]</eval>
    <eval phase="Render" priority="20000" index="2"><![CDATA[field[abSource].text = "Deformity"]]></eval>
    <pickreq thing="fMSRDUtImS" ispreclude="yes"/>
    <pickreq thing="rcMSRDUIm2" ispreclude="yes"/>
    </thing>
  <thing id="rcMSRDWbD2" name="Webbed Digits" description="You grow webbing between your fingers or toes and can move more easily through liquids.\n\n{b}Benefit{/b}: You gain a +4 mutation bonus on all Swim checks, and a +2 to CMD against bull rush, disarm, drag, reposition, and trip attempts. Having webbed digits does not interfere with your ability to grasp or manipulate objects." compset="RaceCustom" uniqueness="unique">
