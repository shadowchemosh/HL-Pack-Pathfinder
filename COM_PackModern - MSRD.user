    <tag group="abRange" tag="Personal"/>
    <tag group="abDuration" tag="Constant"/>
    <bootstrap thing="xFly">
      <autotag group="Maneuver" tag="Average"/>
      <autotag group="Value" tag="60"/>
      </bootstrap>
    <bootstrap thing="wWing">
      <autotag group="Value" tag="2"/>
      <autotag group="wMain" tag="1d2"/>
      </bootstrap>
    <eval phase="Render" priority="20000"><![CDATA[field[abSource].text = "Mutation"]]></eval>
    </thing>
  <thing id="rcMSRDXrVs" name="X-Ray Vision" description="You can see into and through solid matter.\n\n{b}MP Cost{/b}: 6.\n\n{b}Benefit{/b}: X-ray vision allows you to see through 1 foot of brick or stone, 1 inch of metal or composite alloy, and up to 3 feet of wood, plaster, or dirt. Thicker substances or a thin sheet of lead blocks your vision. X-ray vision has a maximum range of 20 feet and allows you to see as if you were looking at something in normal light, even if there is no illumination." compset="RaceCustom" summary="You can see into and through solid matter." uniqueness="unique">
    <fieldval field="abRange" value="20"/>
    <usesource source="GRCMP2MSR"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomCost" tag="6"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="abAction" tag="Constant"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
    <tag group="abRange" tag="Feet"/>
    <bootstrap thing="raXRayVisi">
      <autotag group="Value" tag="20"/>
      <assignval field="abSource" value="Mutation"/>
      </bootstrap>
    <eval phase="Render" priority="20000"><![CDATA[field[abSource].text = "Mutation"]]></eval>
    </thing>
  <thing id="abMSRDCyTr" name="Cybernetic Threshold" description="How many Cybernetic attachments you my still take before taking negative levels." compset="Ability" uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <eval phase="Final" priority="10000"><![CDATA[
      field[abValue].value += 1 + hero.child[aCON].field[aModBonus].value

      field[livename].text = "Cybernetic Threshold (Base Score " & field[abValue].value & ")"]]></eval>
    </thing>
  <thing id="fMSRDCybSg" name="Cybernetic Surgery" description="You can graft cybernetic attachments onto living tissue as well as safely remove them.\n\n{b}Benefit{/b}: You can make a Heal check (DC 20) to install or remove a cybernetic attachment. If you do not have a surgery kit or access to a medical facility, you take a –4 penalty on the check. Cybernetic surgery takes 1d4 hours.\n\nThe consequences of failure are severe: If your check result fails by 5 or more, the installation or removal of the cybernetic attachment causes undue physical trauma to the patient, who suffers 1d4 points of Constitution damage. If the check result fails by 10 or more, the Constitution damage is treated as Constitution drain instead.\n\nA character who undergoes cybernetic surgery (successful or not) is fatigued for 24 hours. Reduce this time by 2 hours for every point above the DC the surgeon achieves. The period of fatigue can never be reduced below 6 hours in this fashion.\n\n{b}Normal{/b}: Characters without this feat take a –8 penalty on Heal checks made to perform cybernetic surgery (–4 penalty if they have the Surgery feat)." compset="Feat" summary="You can graft cybernetic attachments onto living tissue." uniqueness="useronce">
    <usesource source="GRCMP2ScF"/>
    <usesource source="MSRDFutur7"/>
    <usesource source="MSRDFutur8"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="fCategory" tag="General"/>
    <pickreq thing="fMHSurgery"/>
    <exprreq message="Heal 5 ranks"><![CDATA[#skillranks[skHeal] >= 5]]></exprreq>
    </thing>
  <thing id="fMSRDCybTk" name="Cybertaker" description="You can have more cybernetic attachments than normal without suffering ill effects.\n\n{b}Benefit{/b}: The maximum number of cybernetic attachments you can have without suffering negative levels increases by 1.\n\n{b}Special{/b}: You can gain this feat multiple times. Its effects stack." compset="Feat" summary="You can have more cybernetic attachments than normal.">
    <fieldval field="shortname" value="Cybertaker"/>
    <usesource source="GRCMP2MSR"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="fCategory" tag="General"/>
    <eval phase="First">#value[abMSRDCyTr] += 1</eval>
    </thing>
  <thing id="skMSRDCmUs" name="Computer Use" description="You’re trained in the operation of computers and modifying or creating software." compset="Skill" uniqueness="unique">
