    <pickreq thing="rcMSRDScSk" ispreclude="yes"/>
    </thing>
  <thing id="rcMSRDTnFC" name="Thin Fur Coat" description="You grow a thin coat of brown or golden-brown fur, similar to that of a small mammal. \n\n{b}MP Cost{/b}: 0. \n\n{b}Benefit{/b}: None. \n\n{b}Special{/b}: A creature with fur, scales, or chitin cannot gain this mutation." compset="RaceCustom" summary="You grow a thin coat of brown or golden-brown fur" uniqueness="unique">
