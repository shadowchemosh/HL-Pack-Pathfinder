    </thing>
  <thing id="rcMSRDUnVc" name="Unnatural Voice" description="Your voice changes in some marked fashion. It might change pitch or become more lyrical, raspy, whispery, or guttural.\n\n{b}MP Cost{/b}: 0.\n\n{b}Benefit{/b}: None." compset="RaceCustom" uniqueness="unique">
    <usesource source="GRCMP2MSR"/>
    <tag group="AllowRCust" tag="cfgMSRDMut"/>
