    <tag group="SpecSource" tag="cHelpMSg"/>
    <tag group="Helper" tag="Secondary"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="SpecType" tag="Skill"/>
    <tag group="SpecSource" tag="arMSRDStrO"/>
    <eval phase="PostAttr" priority="10100" index="2">hero.child[cMSRDExEff].field[abValue].value += 2

perform hero.childfound[cMSRDImExE].delete[Helper.ShowSpec]</eval>
    <eval phase="First"><![CDATA[
foreach pick in hero from BaseSkill
  if (eachpick.islinkage[skillattr] = 0) then
    ~ do nothing
  elseif (eachpick.linkage[skillattr].tagis[thingid.aSTR] <> 0) then
    eachpick.field[Penalty].value += 2
    endif
  nexteach]]></eval>
    <pickreq thing="cMSRDImExE"/>
    </thing>
  <thing id="cMSRDIgnHd" name="Ignore Hardness" description="The Strong hero has an innate talent for finding weaknesses in objects. This allows a Strong hero to ignore some of an object&apos;s hardness when making a melee attack to break it.\n\nThe Strong hero ignores 2 points of an object&apos;s hardness for each time this Talent has been selected." compset="CustomSpec" summary="Cumulatively ignores an object&apos;s hardness">
    <usesource source="GRCMP2tMP3"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="SpecSource" tag="cHelpMSg"/>
    <tag group="Helper" tag="Secondary"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="SpecType" tag="Attack"/>
    <tag group="SpecSource" tag="arMSRDStrO"/>
    <eval phase="PostLevel" priority="10000"><![CDATA[      ~ see if a quickfind has been created by another copy of this ability
      perform quickfind.setfocus

      if (state.isfocus = 0) then
            perform quickadd
            perform assign[Helper.FirstCopy]
            field[abValue].value = 2

            field[livename].text = field[thingname].text & " (Ignore " & field[abValue].value & " Hardness)"

      else
            perform focus.redirect
            perform assign[Helper.SpecUp]

            focus.field[abValue].value += 2

            hero.child[cMSRDIgnHd].field[livename].text = field[thingname].text & " (Ignore " & focus.field[abValue].value & " Hardness)"
            field[livename].text = field[thingname].text & " Specialization"
            field[abSumm].text = "Increase Ignore Hardness total by 2"
        endif]]></eval>
    </thing>
