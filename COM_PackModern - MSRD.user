  <thing id="rcMSRDAcS2" name="Acidic Saliva" description="Your saliva can burn other creatures like acid. \n\n{b}Benefit{/b}: Your bite attack deals an extra 1d4 points of acid damage with each successful hit.\n\n{b}Special{/b}: You cannot gain this mutation if you have the Venomous Bite mutation." compset="RaceCustom" summary="Your saliva can burn other creatures like acid." uniqueness="unique">
    <fieldval field="shortname" value="Acidic Saliva"/>
    <usesource source="GRCMP2ScF"/>
    <usesource source="MSRDFutur7"/>
    <usesource source="MSRDFutur8"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="Helper" tag="Secondary"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="AllowRCust" tag="cfgMutant"/>
    <eval phase="Final" priority="10000"><![CDATA[
      ~ If we're disabled, just get out now
      doneif (tagis[Helper.SpcDisable] <> 0)

      foreach pick in hero from BaseWep where "thingid.wBite"
        if (length(eachpick.field[wDamExtra].text) = 0) then
          #extradamwithavg[eachpick," plus 1d4 acid",field[thingname].text,"3.5"]
        else
          #extradamwithavg[eachpick," and 1d4 acid",field[thingname].text,"3.5"]
          endif
        nexteach]]></eval>
    <eval phase="Render" priority="20000" index="2"><![CDATA[field[abSource].text = "Mutation"]]></eval>
    <pickreq thing="wBite"/>
    <pickreq thing="rcMSRDVnBt" ispreclude="yes"/>
    </thing>
  <thing id="rcMSRDAdJ2" name="Adrenaline Jolt" description="You can flood your bloodstream with extreme amounts of adrenaline to temporarily boost your Strength and Dexterity. \n\n{b}Benefit{/b}: Once per day, as a free action, you can temporarily increase your Strength and Dexterity by 4 points. The increase lasts for a number of rounds equal to 3 + your Constitution modifier." compset="RaceCustom" summary="Temporarily boost your Strength and Dexterity." uniqueness="unique">
    <usesource source="GRCMP2ScF"/>
    <usesource source="MSRDFutur7"/>
    <usesource source="MSRDFutur8"/>
    <tag group="User" tag="Tracker"/>
    <tag group="Usage" tag="DayRound"/>
    <tag group="AllowRCust" tag="cfgMutant"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="abAction" tag="Free"/>
    <tag group="Helper" tag="Secondary"/>
    <tag group="User" tag="Activation"/>
    <tag group="AbilType" tag="Extra"/>
    <eval phase="Render" priority="20000" index="3"><![CDATA[field[abSource].text = "Mutation"]]></eval>
    <eval phase="PostAttr" priority="10000">~uses per day = 3 + CON mod
      field[trkMax].value += 3 + hero.child[aCON].field[aModBonus].value</eval>
    <eval phase="Level" priority="1000" index="2"><![CDATA[
      if (field[abilActive].value <> 0) then

        #applybonus[ModCirc,hero.child[aSTR], 4]
        #applybonus[ModCirc,hero.child[aDEX], 4]
        endif]]></eval>
    </thing>
  <thing id="rcMSRDBld2" name="Blood Hunger" description="You crave the taste of blood. Moreover, you need to drink blood to survive. \n\n{b}Drawback{/b}: You must drain a pint of blood from a living creature once every 24 hours. Doing so is an attack action, and you can only drain blood from a willing, helpless, or dying (but not dead) creature. The bitten creature takes normal damage from the bite attack plus an extra 1d6 points of damage from the blood loss. \n\nIf you go 24 hours without consuming blood, you take 1d4 points of Constitution damage. Drinking a pint of blood cures the ability damage caused by blood deprivation in 1d6 rounds. Ability damage caused by blood deprivation cannot be restored through natural healing. \n\n{b}Special{/b}: You must have a natural bite attack to take this drawback." compset="RaceCustom" summary="You crave the taste of blood, you need to drink blood to survive." uniqueness="unique">
    <usesource source="GRCMP2ScF"/>
    <usesource source="MSRDFutur7"/>
    <usesource source="MSRDFutur8"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="SpecType" tag="Weakness"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="AllowRCust" tag="cfgMutant"/>
    <eval phase="Final" priority="10000"><![CDATA[
      ~ If we're disabled, just get out now
      doneif (tagis[Helper.SpcDisable] <> 0)

      foreach pick in hero from BaseWep where "thingid.wBite"
        if (length(eachpick.field[wDamExtra].text) = 0) then
          #extradamwithavg[eachpick," plus 1d6 blood loss",field[thingname].text,"3.5"]
        else
          #extradamwithavg[eachpick," and 1d6 blood loss",field[thingname].text,"3.5"]
          endif
        nexteach]]></eval>
    <eval phase="Render" priority="20000" index="2"><![CDATA[field[abSource].text = "Deformity"]]></eval>
    <pickreq thing="wBite"/>
    </thing>
  <thing id="rcMSRDBrB2" name="Brittle Bones" description="Your bones weaken such that you can no longer withstand hard or sudden impacts. \n\n{b}Drawbacks{/b}: Your Hit Pont Total decreases by 5. In addition, you take an additional 1d6 points of damage from a fall. \n\n{b}Special{/b}: You cannot take this drawback if you have the Skeletal Reinforcement mutation." compset="RaceCustom" summary="Your Hit Pont Total decreases by 5." uniqueness="unique">
    <usesource source="GRCMP2ScF"/>
    <usesource source="MSRDFutur7"/>
    <usesource source="MSRDFutur8"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="SpecType" tag="Weakness"/>
    <tag group="AllowRCust" tag="cfgMutant"/>
    <tag group="AbilType" tag="Extra"/>
