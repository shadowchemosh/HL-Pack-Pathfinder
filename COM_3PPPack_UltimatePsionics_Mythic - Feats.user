    <exprreq message="Unwilling Participant feat required."><![CDATA[#hasfeat[fPUUnwillP] <> 0]]></exprreq>
    <exprreq message="2nd Mythic Tier required."><![CDATA[#tierprereq[] >= 2]]></exprreq>
