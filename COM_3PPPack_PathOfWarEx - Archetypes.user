    <bootstrap thing="cPWWarPat1">
      <autotag group="ClSpecWhen" tag="1"/>
      </bootstrap>
    <bootstrap thing="cPWWarPat2">
      <autotag group="ClSpecWhen" tag="1"/>
      </bootstrap>
