      #skillbonus[skDisguise] += field[abValue].value
      #skillbonus[skEscape] += field[abValue].value
      #skillbonus[skIntim] += field[abValue].value
      #skillbonus[skLinguist] += field[abValue].value
      #skillbonus[skSenseMot] += field[abValue].value
      #skillbonus[skSleight] += field[abValue].value
      #skillbonus[skSpellcr] += field[abValue].value
