  <thing id="cMPCHIpWD" name="Improvised Weapon Damage" description="At 12th level, an Infiltrator&apos;s attacks with improvised weapons deal more damage.\n\nThe Infiltrator treats an improvised weapon as one size category larger than it is for the purpose of determining the damage it deals. \n\nThis ability replaces trap sense +4." compset="ClSpecial">
    <usesource source="GRCMP2tMP3"/>
    <tag group="AbilType" tag="Extra" name="Extraordinary Ability" abbrev=" (Ex)"/>
    <eval phase="First" priority="11000"><![CDATA[
      ~ If we're disabled, do nothing
      doneif (tagis[Helper.FtDisable] <> 0)

      foreach pick in hero from BaseWep where "Helper.Improvised"
        ~ Set our critical modifier to it's expanded range.
        perform eachpick.assign[wCritMin.19]

        ~ We need to increase our damage if we are currently below 1d8 (2d6 if 
        ~ two handed).
        if (eachpick.tagis[wClass.TwoHanded] <> 0) then
          if (eachpick.tagvalue[wMain.?] < 104) then
            perform eachpick.assign[Helper.DamageUp]

          ~ We need a specific check for the 1d10 tag, since that is on another
          ~ progression level.
          elseif (eachpick.tagvalue[wMain.?] = 304) then
            perform eachpick.assign[Helper.DamageUp]
            endif
        else
          if (eachpick.tagvalue[wMain.?] < 6) then
            perform eachpick.assign[Helper.DamageUp]
            endif
          endif
        nexteach]]></eval>
    </thing>
  <thing id="cMPCHIIpWD" name="Improvised Weapon Damage" description="An Infiltrator&apos;s attacks with improvised weapons deal more damage.\n\nThe Infiltrator treats an improvised weapon as one size category larger than it is for the purpose of determining the damage it deals." compset="CustomSpec" uniqueness="useronce">
