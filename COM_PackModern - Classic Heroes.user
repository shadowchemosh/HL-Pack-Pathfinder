  <thing id="cMPCHUnCSv" name="Uncanny Survival" description="Beginning at 3rd level, a Field Officer can add one-half his class level to his Armor Class for 1 round, once per day. \n\n(The bonus applies to the Field Officer&apos;s touch and flat-footed Armor Class, as well.) He must declare he is doing this at the beginning of his turn, and the Armor Class bonus lasts until his next round of actions. \n\nThis ability replaces Cavalier&apos;s charge." compset="ClSpecial">
    <usesource source="GRCMP2tMP3"/>
    <tag group="AbilType" tag="Extra"/>
    <eval phase="PostLevel" priority="10000"><![CDATA[      ~ If we're not shown, just get out now
      doneif (tagis[Helper.ShowSpec] = 0)

      ~our bonus is level /2, min 1
      var bonus as number
      bonus = field[xTotalLev].value / 2
      field[abValue].value += maximum(round(bonus,0,-1),1)
      field[livename].text = field[thingname].text & " +" & field[abValue].value
      field[abSumm].text = "Your Uncanny Survival Bonus is +" & field[abValue].value]]></eval>
    </thing>
  <thing id="cMPCHUnCSu" name="Uncanny Survival" description="As a free action, a Field Officer can add 1/2 class level to Armor Class for one round for each rank in Knowledge (tactics) per day." compset="CustomSpec" summary="Add one-half his class level to his Armor Class ." uniqueness="useronce">
    <fieldval field="actName" value="Armor Class bonus"/>
    <usesource source="GRCMP2tMP3"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="User" tag="Activation"/>
    <tag group="Helper" tag="Secondary"/>
    <tag group="User" tag="Tracker"/>
    <tag group="Usage" tag="Day"/>
    <tag group="SpecSource" tag="arMPCHFldO"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <eval phase="PostLevel" priority="10000" index="2"><![CDATA[if (field[abilActive].value <> 0) then

      var bonus as number
      bonus = field[xTotalLev].value / 2
      field[abValue2].value += maximum(round(bonus,0,-1),1)
      hero.child[ArmorClass].field[Bonus].value += field[abValue2].value
endif]]></eval>
    <eval phase="PostAttr" priority="10000" index="3">field[trkMax].value += #skillranks[skMHTactic]</eval>
    <eval phase="PostLevel" priority="10000"><![CDATA[      ~our bonus is level /2, min 1
      var bonus as number
      bonus = field[xTotalLev].value / 2
      field[abValue].value += maximum(round(bonus,0,-1),1)
      field[livename].text = field[thingname].text & " +" & field[abValue].value
