      </bootstrap>
    </thing>
  <thing id="cMPCHImpWn" name="Improvised Weapons" description="The Street Warrior becomes an expert at using improvised weapons. \n\nThe Street Warrior does not take a -4 penalty on attack rolls when wielding an improvised weapon." compset="CustomSpec" uniqueness="useronce">
