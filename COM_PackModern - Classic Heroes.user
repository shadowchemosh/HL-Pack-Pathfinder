    <eval phase="Final" priority="10000"><![CDATA[      ~ our DC is equal to the channel energy's DC
      field[abDC].value += hero.childfound[xChannel].field[abDC].value]]></eval>
    <exprreq message="Ability to channel energy required."><![CDATA[hero.tagis[Ability.xChannel] <> 0]]></exprreq>
    </thing>
  <thing id="cMPCHArcSk" name="Arcane Skills" description="You gain a competence bonus to all the following skill checks equal to half your level.\n\nConcentration, Craft (chemical), Linguistics, and Spellcraft." compset="CustomSpec" uniqueness="useronce">
