    <eval phase="Render" priority="10000" index="2"><![CDATA[
      ~if we're not shown, just get out now
      doneif (tagis[Helper.ShowSpec] = 0)

      ~only do the rest if we're the first copy
      doneif (tagis[Helper.FirstCopy] = 0)

      if (field[abValue].value = field[abValue2].value) then
        field[livename].text = "Trap Sense " & signed(field[abValue].value)
        field[abSumm].text = "+" & field[abValue].value & " bonus on reflex saves and AC against traps."
      else
        field[livename].text = "Trap Sense " & signed(field[abValue].value) & "/" & signed(field[abValue2].value)
        field[abSumm].text = "+" & field[abValue].value & " bonus on reflex saves and " & signed(field[abValue2].value) & " AC against traps."
        endif
      #situational[hero.child[svRef],signed(field[abValue].value) & " bonus vs. traps",field[thingname].text]
      #situational[hero.child[ArmorClass],signed(field[abValue2].value) & " bonus vs. traps",field[thingname].text]]]></eval>
    <eval phase="PostLevel" priority="10000"><![CDATA[
      field[listname].text = "Trap Sense +" & field[xIndex].value

      ~if we're not shown, just get out now
      doneif (tagis[Helper.ShowSpec] = 0)

      field[abValue].value += field[xCount].value
      field[abValue2].value += field[abValue].value]]></eval>
    </thing>
