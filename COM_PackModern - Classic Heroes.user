    <tag group="SpecType" tag="Defense" name="Defense" abbrev="Defense"/>
    <tag group="AbilType" tag="Extra" name="Extraordinary Ability" abbrev=" (Ex)"/>
    <tag group="Helper" tag="SpecUp"/>
    <eval phase="PostAttr" priority="5000"><![CDATA[
      ~only perform the calculations for the first copy
      doneif (tagis[Helper.FirstCopy] = 0)

      ~ If we're not shown, just get out now
      doneif (tagis[Helper.ShowSpec] = 0)

      var bonus as number
      bonus = hero.child[aINT].field[aModBonus].value
      hero.child[ArmorClass].field[tACDexMod].value += bonus

      field[livename].text = "Smart Defense +" & field[abValue].value]]></eval>
    </thing>
