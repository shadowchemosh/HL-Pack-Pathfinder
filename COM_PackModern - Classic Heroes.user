    <tag group="AbilType" tag="PsiLike"/>
    <tag group="Helper" tag="UserAdjust"/>
    <tag group="Custom" tag="MPSGGPsych"/>
    <tag group="AllowRCust" tag="cfgMPPsyTa"/>
