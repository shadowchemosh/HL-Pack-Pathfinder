    <eval phase="Final" priority="10000" index="3"><![CDATA[if (field[abilActive].value <> 0) then
  hero.child[ArmorClass].field[Bonus].value += 5
endif]]></eval>
    <eval phase="PreAttr" priority="10000" index="4"><![CDATA[if (field[abilActive].value <> 0) then
#applybonus[aPostMod, hero.child[aSTR], 4]
#applypenalty[aPostMod, hero.child[aDEX], -2]
endif]]></eval>
    <eval phase="PostLevel" priority="3000" index="5"><![CDATA[if (field[abilActive].value <> 0) then    
  foreach pick in hero from BaseArmor where "EquipType.Armor"
        eachpick.field[arArmorChk].value -= 4
        nexteach
        endif]]></eval>
    <eval phase="PreLevel" priority="2500" index="6"><![CDATA[if (field[abilActive].value <> 0) then       
 foreach pick in hero where "EquipType.Armor"
          if (eachpick.field[gIsEquip].value <> 0) then
            eachpick.field[arArcFail].value += 25
