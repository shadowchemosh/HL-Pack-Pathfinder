  <thing id="cMPCHDwPw" name="Draw Power" description="A Psi Agent can spend a hero point to regain a number of psi points equal to her levels in this class. This is a standard action." compset="CustomSpec" summary="Spend a hero point to regain a number of Psi Points" uniqueness="unique">
