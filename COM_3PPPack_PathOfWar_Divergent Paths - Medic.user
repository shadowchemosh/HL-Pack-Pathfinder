  <thing id="cPW7DevImm" name="Developed Immunity" description="The medic becomes immune to all poisons and nonmagical diseases." compset="CustomSpec" summary="Gain immunity to poison and nonmagical disease" uniqueness="unique">
    <usesource source="srcDPthMed"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="SpecSource" tag="cHelpPW7"/>
    <bootstrap thing="xImmDis"></bootstrap>
    <bootstrap thing="xImmPois"></bootstrap>
    <exprreq message="Medic level 8 required."><![CDATA[#custspeciallevelcount[PWMedic] >= 8]]></exprreq>
    </thing>
  <thing id="cPW7DocAdv" name="Doctor&apos;s Advice" description="Whenever the medic successfully uses the aid another action, she adds +2 to the bonus provided." compset="CustomSpec" summary="+2 to your Aid Another action bonus" uniqueness="unique">
    <usesource source="srcDPthMed"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="SpecSource" tag="cHelpPW7"/>
    <exprreq message="Medic level 4 required."><![CDATA[#custspeciallevelcount[PWMedic] >= 4]]></exprreq>
    </thing>
  <thing id="cPW7EmbBoo" name="Emboldening Boost" description="When the medic initiates a boost, one ally she can see within 30 feet gains temporary hit points equal to her medic initiator level for one round." compset="CustomSpec" summary="Grant temporary hit points when you initiate a boost" uniqueness="unique">
    <fieldval field="abRange" value="30"/>
    <usesource source="srcDPthMed"/>
    <tag group="abRange" tag="Feet"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="SpecSource" tag="cHelpPW7"/>
    </thing>
  <thing id="cPW7FirRes" name="First Responder" description="The medic may instead move up to twice her speed when using her triage ability. If the medic is using her triage ability as a full round action, she can instead move up to triple her speed. This does not count towards the limit of expertises she can apply to her triage ability." compset="CustomSpec" summary="Move farther when you use triage" uniqueness="unique">
    <usesource source="srcDPthMed"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="SpecSource" tag="cHelpPW7"/>
    </thing>
  <thing id="cPW7ForTou" name="Fortifying Touch" description="The target of the medic’s triage ability gains a bonus to its next saving throw this round equal to the medic’s initiation modifier." compset="CustomSpec" summary="Grant a save bonus when you use triage" uniqueness="unique">
    <usesource source="srcDPthMed"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="SpecSource" tag="cHelpPW7"/>
    </thing>
  <thing id="cPW7InvTou" name="Invigorating Touch" description="Instead of healing the normal amount of hit points, the target of the medic’s triage ability gains fast healing equal to the medic’s initiation modifier for 1 minute." compset="CustomSpec" summary="You grant fast healing instead of healing hp with triage" uniqueness="unique">
    <fieldval field="abDuration" value="1"/>
    <usesource source="srcDPthMed"/>
    <tag group="abDuration" tag="Minute"/>
    <tag group="DescInfo" tag="raFastHea"/>
    <tag group="SpecSource" tag="cHelpPW7"/>
    <tag group="Helper" tag="NoPathSoc"/>
    </thing>
  <thing id="cPW7MarHea" name="Martial Healer" description="When the medic successfully hits with a strike, she may spend a triage as a swift action to heal an ally within 30 feet. This use of triage only heals 1/2 the normal amount of damage." compset="CustomSpec" summary="When you hit with a strike, use triage with lesser healing" uniqueness="unique">
    <fieldval field="abRange" value="30"/>
    <usesource source="srcDPthMed"/>
    <tag group="abRange" tag="Feet"/>
    <tag group="SpecSource" tag="cHelpPW7"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="abAction" tag="Swift"/>
    </thing>
  <thing id="cPW7MirSur" name="Miracle Surgeon" description="As part of administering long term care, the medic can reattach severed limbs or organs to a body. The target creature must be living, and the limb cannot have been separated from the patient for more than a number of hours equal to the medic&apos;s class level. The overall process takes the full 8 hours of providing long term care." compset="CustomSpec" summary="Reattach limbs or organs as part of long-term care" uniqueness="unique">
    <fieldval field="abDuration" value="8"/>
    <usesource source="srcDPthMed"/>
    <tag group="SpecSource" tag="cHelpPW7"/>
    <tag group="abDuration" tag="Hour"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="abAction" tag="None"/>
    <exprreq message="Medic level 10 required."><![CDATA[#custspeciallevelcount[PWMedic] >= 10]]></exprreq>
    </thing>
  <thing id="cPW7NeuPoi" name="Neutralize Poison" description="The medic may neutralize one poison affecting the target of her triage ability, as the spell {i}neutralize poison{/i}. The medic uses her initiator level in place of the caster level for this effect." compset="CustomSpec" summary="Apply {i}neutralize poison{/i} as part of triage" uniqueness="unique">
    <usesource source="srcDPthMed"/>
    <tag group="SpInfo" tag="spNeutPoi3"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="SpecSource" tag="cHelpPW7"/>
    <exprreq message="Medic level 8 required."><![CDATA[#custspeciallevelcount[PWMedic] >= 8]]></exprreq>
    </thing>
  <thing id="cPW7DelPoi" name="Poison Knowledge" description="The medic never risks poisoning herself when applying or crafting poisons. In addition, she may delay the effects of one poison affecting the target of her triage ability for 1 hour as the spell {i}delay poison{/i}." compset="CustomSpec" summary="Use poison with no risk, and apply {i}delay poison{/i} as part of triage" uniqueness="unique">
    <usesource source="srcDPthMed"/>
    <tag group="SpInfo" tag="spDelaPoi2"/>
    <tag group="SpInfo" tag="spDelaPoi1"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="SpecSource" tag="cHelpPW7"/>
    </thing>
  <thing id="cPW7RecTou" name="Recovering Touch" description="The medic may remove one of the following conditions from the target of her triage ability: fatigued, sickened, staggered, or shaken." compset="CustomSpec" summary="Remove conditions as part of triage" uniqueness="unique">
    <usesource source="srcDPthMed"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="SpecSource" tag="cHelpPW7"/>
    </thing>
  <thing id="cPW7RToImp" name="Recovering Touch, Improved" description="The medic may remove up to two conditions with her recovering touch expertise. In addition, she adds the following conditions to the list of conditions her recovering touch can remove: blinded, dazed, deafened, exhausted, frightened, nauseated, paralyzed or stunned." compset="CustomSpec" summary="Remove more conditions as part of triage" uniqueness="unique">
    <fieldval field="sbName" value="improved recovering touch"/>
    <usesource source="srcDPthMed"/>
    <tag group="SpecSource" tag="cHelpPW7"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <exprreq message="Medic level 6 required."><![CDATA[#custspeciallevelcount[PWMedic] >= 6]]></exprreq>
    <exprreq message="Recovering Touch required."><![CDATA[#hasability[cPW7RecTou] <> 0]]></exprreq>
    </thing>
  <thing id="cPW7Regrou" name="Regroup" description="When the medic uses her triage ability on herself, she can move up to her speed. This movement does not provoke attacks of opportunity. This does not count towards the maximum number of medic’s expertises that she can use with her triage ability." compset="CustomSpec" summary="Move further when you use triage on yourself" uniqueness="unique">
    <usesource source="srcDPthMed"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="SpecSource" tag="cHelpPW7"/>
    <exprreq message="Medic level 10 required."><![CDATA[#custspeciallevelcount[PWMedic] >= 10]]></exprreq>
    </thing>
  <thing id="cPW7RevTou" name="Revitalizing Touch" description="The target of the medic’s triage ability heals an amount of ability score damage equal to the medic’s initiation modifier instead of healing hit point damage. This healing may be divided up as the medic chooses between multiple ability scores as necessary." compset="CustomSpec" summary="Heal ability point damage as part of triage" uniqueness="unique">
    <usesource source="srcDPthMed"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="SpecSource" tag="cHelpPW7"/>
    </thing>
  <thing id="cPW7TreCur" name="Treat Curse" description="The medic may attempt to remove one curse affecting the target of her triage ability as the spell {i}remove curse{/i}. The medic uses her initiator level in place of the caster level for the caster level check of the spell." compset="CustomSpec" summary="Apply {i}remove curse{/i} as part of triage" uniqueness="unique">
    <usesource source="srcDPthMed"/>
    <tag group="SpInfo" tag="spRemoCur3"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="SpecSource" tag="cHelpPW7"/>
    </thing>
  <thing id="cPW7TreDis" name="Treat Disease" description="The medic may remove one disease affecting the target of her triage ability as the spell {i}remove disease{/i}. The medic uses her initiator level in place of the caster level for the caster level check of the spell." compset="CustomSpec" summary="Apply {i}remove disease{/i} as part of triage" uniqueness="unique">
    <usesource source="srcDPthMed"/>
    <tag group="SpInfo" tag="spRemoDis2"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="SpecSource" tag="cHelpPW7"/>
    <exprreq message="Medic level 4 required."><![CDATA[#custspeciallevelcount[PWMedic] >= 4]]></exprreq>
    </thing>
  <thing id="cPW7WraHea" name="Wrathful Healing" description="When the medic uses her triage ability to heal an ally, the medic deals additional damage on the next strike she initiates within one round equal to her medic initiation modifier." compset="CustomSpec" summary="When you use triage, add damage to your next strike" uniqueness="unique">
    <usesource source="srcDPthMed"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="SpecSource" tag="cHelpPW7"/>
    </thing>
  <thing id="cPW7CurIps" name="Cura Te Ipsum" description="Starting at 3th level, the medic’s martial training enhances her ability to defend herself while she ensures the health and safety of her allies. Whenever the medic initiates a strike maneuver she gains a +1 insight bonus on Reflex and Will saves until the start of her next turn. This bonus increases by +1 at 7th level and again at every four levels thereafter." compset="ClSpecial" summary="Get a bonus on Reflex and Will saves after initiating a strike">
    <tag group="SpecType" tag="Defense"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="User" tag="Activation"/>
    <tag group="SbNameApp" tag="AppValue"/>
    <tag group="SbNameApp" tag="SignAppVal"/>
    <tag group="LvNameApp" tag="AppValue"/>
    <tag group="LvNameApp" tag="SignAppVal"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <eval phase="PostLevel" priority="10000"><![CDATA[
      ~ Set the list name
      field[listname].text = field[thingname].text & " " & signed(field[xIndex].value)

      ~ If we're not shown, just get out now
      doneif (tagis[Helper.ShowSpec] <> 1)
      ~ if we've been disabled, get out now
      doneif (tagis[Helper.SpcDisable] <> 0)

      ~ Set the insight bonus here
      field[abValue].value += field[xCount].value

      ~ The following should only happen once on the first copy
      doneif (tagis[Helper.FirstCopy] = 0)

      ~ If activated give bonus to saves
      If (field[abilActive].value = 1) then
        #applybonus[BonInsight,hero.child[svWill],field[abValue].value]
        #applybonus[BonInsight,hero.child[svRef],field[abValue].value]

      ~.. If not active set situational note
      Else
        field[abText].text = "When you initiate a medic strike maneuver you gain a " & signed(field[abValue].value) & " insight bonus until the start of your next turn."
        #situational[hero.child[svWill],field[abText].text,field[name].text]
        #situational[hero.child[svRef],field[abText].text,field[name].text]
      Endif]]></eval>
    </thing>
  <thing id="cPW7ImpTre" name="Improved Treatment" description="Starting at 2nd level, when the medic treats deadly wounds, the creature she treats recovers hit points and ability score damage as if it had rested for a full day and she no longer needs a healer’s kit to perform these actions. At 6th level, the medic can treat deadly wounds or provide long term care to a number of creatures equal to her initiation modifier at the same time. In addition, when the medic treats a creature’s deadly wounds it recovers hit points as if it had rested for a full day with long term care. At 11th level, when the medic treat deadly wounds, the creature recovers hit point and ability damage as if it had rested for 3 days." compset="ClSpecial" summary="More benefits when you treat deadly wounds">
    <tag group="AbilType" tag="Extra"/>
    <tag group="ProductId" tag="HLCommunit"/>
    </thing>
  <thing id="cPW7ImpTri" name="Improved Triage" description="At 5th level, the medic can apply the effect of two medic’s expertises of her choice to any use of her Triage ability. At 10th level, she may apply the effect of a third medic’s expertise, and at 15th level, a fourth." compset="ClSpecial" summary="Apply more medic&apos;s expertises to triage">
    <fieldval field="abValue" value="1"/>
    <fieldval field="abText" value="expertises"/>
    <tag group="SbNamePar" tag="AppText"/>
    <tag group="SbNamePar" tag="AppValue"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="LvNamePar" tag="AppText"/>
    <tag group="LvNamePar" tag="AppValue"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <eval phase="PostLevel" priority="10000"><![CDATA[
      ~ Set the list name
      field[listname].text = field[thingname].text & " (" & field[xIndex].value & " expertises)"

      ~ If we're not shown, just get out now
      doneif (tagis[Helper.ShowSpec] <> 1)
      ~ if we've been disabled, get out now
      doneif (tagis[Helper.SpcDisable] <> 0)

      field[abValue].value += field[xCount].value
      field[livename].text = field[thingname].text & " (" & field[abValue].value & " expertises)"]]></eval>
    </thing>
  <thing id="fPWDanShie" name="Dancing Shield Step" description="You have learned to use the momentary disorientation of your opponents after a shield bash to gracefully reposition yourself.\n\n{b}Prerequisites:{/b} Improved Shield Bash, Quick Draw, two Iron Tortoise maneuvers known, two Mithral Current maneuvers known.\n\n{b}Benefits:{/b} You can don a shield as a free action. In addition, while in a Mithral Current stance, whenever you hit an opponent with a shield bash or successfully use a counter that requires a shield bash, you can take a free 5 foot step." compset="Feat" summary="In a Mithral Current stance, you can take a 5-foot step after a shield bash" uniqueness="useronce">
    <usesource source="srcDPthMed"/>
    <tag group="SpecType" tag="MoveOther"/>
    <tag group="fCategory" tag="Combat"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <exprreq message="Must know two Mithral Current maneuvers."><![CDATA[hero.tagcount[HasMan.MithCurr] >= 2]]></exprreq>
    <exprreq message="Must know two Iron Tortoise maneuvers."><![CDATA[hero.tagcount[HasMan.IronTort] >= 2]]></exprreq>
    <exprreq message="Improved Shield Bash feat required."><![CDATA[#hasfeat[fImpShBash] <> 0]]></exprreq>
    <exprreq message="Quick Draw feat required."><![CDATA[#hasfeat[fQuickDraw] <> 0]]></exprreq>
    </thing>
  <thing id="fPWExGuaLi" name="Extra Guardian of Life" description="You are better at preparing your allies for unexpected emergencies.\n\n{b}Prerequisites:{/b} Guardian of life class feature, initiator level 9th.\n\n{b}Benefit:{/b} You can use guardian of life one additional time per day." compset="Feat" summary="Gain an additional use of guardian of life" uniqueness="useronce">
    <usesource source="srcDPthMed"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <eval phase="PostLevel" priority="10000"><![CDATA[
      ~ If we're disabled, do nothing
      doneif (tagis[Helper.FtDisable] <> 0)

      ~ add 1 use to guardian of life ability
      #trkmax[cPW7GuaLif] += 1]]></eval>
    <exprreq message="Guardian of life class feature"><![CDATA[#hasability[cPW7GuaLif] <> 0]]></exprreq>
    <exprreq message="initiator level 9th required."><![CDATA[hero.childfound[mHelpPW7].field[cIL].value >= 9]]></exprreq>
    </thing>
  <thing id="fPWExMedEx" name="Extra Medic&apos;s Expertise" description="You are better at preparing your allies for unexpected emergencies.\n\n{b}Prerequisites:{/b} Medic’s expertise class feature.\n\n{b}Benefit:{/b} You learn a new medic’s expertise. You must meet the prerequisites for this expertise as normal.\n\n{b}Special:{/b} You can take this feat multiple times. Each time you do, you learn a new medic&apos;s expertise." compset="Feat" summary="Gain an additional medic&apos;s expertise">
    <fieldval field="abValue" value="1"/>
    <usesource source="srcDPthMed"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <eval phase="PreLevel" priority="10000"><![CDATA[
      ~ If we're disabled, do nothing
      doneif (tagis[Helper.FtDisable] <> 0)

      hero.childfound[cHelpPW7].field[cGiveSpec].value += field[abValue].value]]></eval>
    <exprreq message="Medic’s expertise class feature"><![CDATA[hero.childfound[cHelpPW7].field[cGiveSpec].value >= 1]]></exprreq>
    </thing>
  <thing id="fPWFirsAid" name="First Aid Training" description="You have been trained to identify and patch up wounds in preparation for more serious treatment.\n\n{b}Benefit:{/b} When you make a Heal check to perform first aid on a creature, that creature gains temporary hit points equal to your Wisdom modifier plus your ranks in the Heal skill. These temporary hit points last for 1 minute or until lost.\n\n{b}Special:{/b} If you use an ability score other than Wisdom to determine your bonus on Heal checks, you use that ability score in place of Wisdom for determining the number of temporary hit points the target gains." compset="Feat" summary="Grant temporary hit points when you perform first aid" uniqueness="useronce">
    <fieldval field="abText" value="temp hp"/>
    <fieldval field="abDuration" value="1"/>
    <usesource source="srcDPthMed"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="LvNamePar" tag="AppText"/>
    <tag group="LvNamePar" tag="AppValue"/>
    <tag group="SbNamePar" tag="AppText"/>
    <tag group="SbNamePar" tag="AppValue"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="abDuration" tag="Minute"/>
    <tag group="abAction" tag="None"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <eval phase="PostAttr" priority="50000"><![CDATA[
      ~ If we're disabled, do nothing
      doneif (tagis[Helper.FtDisable] <> 0)

      ~ Add in heal skill ranks
      field[abValue].value += hero.childfound[skHeal].field[skRanks].value

      var sTag as string
      ~ Get the ability score related to the heal skill
      sTag = hero.childfound[skHeal].tagids[SkillAbil.?]

      ~ Set pointer to the correct ability score
      perform hero.findchild[BaseAttr,sTag].setfocus
      doneif (state.isfocus = 0)

      ~ Give ability score bonus
      field[abValue].value += focus.field[aModBonus].value]]></eval>
    </thing>
  <thing id="fPWGungTec" name="Gungnir Technique" description="Having trained with numerous demon and ghost hunters you know how to pierce a spirit without harming the host.\n\n{b}Prerequisites:{/b} Two Piercing Thunder maneuvers known, two Silver Crane maneuvers known.\n\n{b}Benefits:{/b} While you maintain a Silver Crane stance, polearms and spears you wield can affect incorporeal creatures as if they possessed the {i}ghost touch{/i} weapon special ability, and you can attack possessing creatures. Whenever you hit a possessed creature with such a weapon, you can choose for your attack to affect the possessing entity rather than the target." compset="Feat" summary="In a Silver Crane stance, you can attack incorporeal and possessing creatures" uniqueness="useronce">
    <usesource source="srcDPthMed"/>
    <tag group="fCategory" tag="Combat"/>
    <tag group="SpecType" tag="Attack"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <exprreq message="Must know two Silver Crane maneuvers."><![CDATA[hero.tagcount[HasMan.SilvrCrane] >= 2]]></exprreq>
    <exprreq message="Must know two Piercing Thunder maneuvers."><![CDATA[hero.tagcount[HasMan.PrcThun] >= 2]]></exprreq>
    </thing>
  <thing id="fPWHealMer" name="Healer&apos;s Mercy" description="Sometimes a creature is beyond your ability to heal, but you know how to grant them a swift and painless death.\n\n{b}Prerequisites:{/b} Heal 5 ranks.\n\n{b}Benefit:{/b} When you use a coup de grace action, you can substitute a Heal check in place of the normal damage roll for your weapon, but still use your weapon’s normal critical multiplier. In addition, if the creature you are coup de gracing has regeneration of a type that can be overcome, you suppress that creatures regeneration for the duration of the round in which you coup de grace them." compset="Feat" summary="Use a Heal check to coup de grace" uniqueness="useronce">
    <usesource source="srcDPthMed"/>
    <tag group="SpecType" tag="Attack"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <exprreq message="Heal 5 ranks required."><![CDATA[#skillranks[skHeal] >= 5]]></exprreq>
    </thing>
  <thing id="fPWImpShie" name="Imposing Shield" description="You are adept at using your shield to distract and control the movements of your enemies.\n\n{b}Prerequisites:{/b} Improved Shield Bash, two Eternal Guardian maneuvers known, two Iron Tortoise maneuvers known\n\n{b}Benefits:{/b} While in an Eternal Guardian stance, if you make a shield bash attack against an enemy, that creature takes a –2 penalty on attack rolls that do not include you as a target until the start of your next turn. This penalty to attack rolls does not stack with other penalties to attack rolls that you apply to the target." compset="Feat" summary="In an Eternal Guardian stance, inflict attack penalties with shield bashes" uniqueness="useronce">
    <usesource source="srcDPthMed"/>
    <tag group="fCategory" tag="Combat"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="SpecType" tag="Attack"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <exprreq message="Must know two Eternal Guardian maneuvers."><![CDATA[hero.tagcount[HasMan.EternGuard] >= 2]]></exprreq>
    <exprreq message="Must know two Iron Tortoise maneuvers."><![CDATA[hero.tagcount[HasMan.IronTort] >= 2]]></exprreq>
    <exprreq message="Improved Shield Bash feat required."><![CDATA[#hasfeat[fImpShBash] <> 0]]></exprreq>
    </thing>
  <thing id="fPWNemLion" name="Nemean Lion Hide" description="The strength of your martial pride is so great that lesser blows fail to even phase you.\n\n{b}Prerequisites:{/b} Martial Power, two Golden Lion maneuvers known, two Iron Tortoise maneuvers known.\n\n{b}Benefit:{/b} Whenever you use the Martial Power feat while maintaining a Golden Lion or Iron Tortoise stance, you also gain damage reduction X/— equal to your highest initiation modifier until the start of your next turn." compset="Feat" summary="In a Golden Lion or Iron Tortoise stance, gain DR when you use Martial Power" uniqueness="useronce">
    <usesource source="srcDPthMed"/>
    <tag group="fCategory" tag="Combat"/>
    <tag group="SpecType" tag="DR"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <exprreq message="Must know two Iron Tortoise maneuvers."><![CDATA[hero.tagcount[HasMan.IronTort] >= 2]]></exprreq>
    <exprreq message="Must know two Golden Lion maneuvers."><![CDATA[hero.tagcount[HasMan.GoldenLion] >= 2]]></exprreq>
    <exprreq message="Martial Power feat required."><![CDATA[#hasfeat[fPWMartPow] <> 0]]></exprreq>
    </thing>
  <thing id="fPWPlaDoc" name="Plague Doctor" description="You know how to treat disease and prevent its spread.\n\n{b}Prerequisites:{/b} Heal 7 ranks.\n\n{b}Benefit:{/b} If you have successfully treated someone for a disease using the Heal skill, you can perform a Heal check on healthy individuals to give them a bonus on saving throws against that disease for one month equal to your Wisdom modifier. The DC of this Heal check is the same as the DC to treat the disease." compset="Feat" summary="Use Heal checks to give save bonuses vs. disease" uniqueness="useronce">
    <usesource source="srcDPthMed"/>
    <tag group="SpecType" tag="Skill"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <exprreq message="Heal 7 ranks required."><![CDATA[#skillranks[skHeal] >= 7]]></exprreq>
    </thing>
  <thing id="ioPWIncCon" name="Incense of Convalescence" description="This carefully grown and regulated incense shields the mind from harm while it recovers from trauma. Once inhaled, the inhaler ignores up to 2, 4 or 6 points of mental ability score damage, drain, or penalties, depending on the amount of incense burned. Once the incense is inhaled, it last for 24 hours before its effects wear off and a new dose must be taken. A typical box of {i}incense of convalescence{/i} contains enough incense to properly protect the body against 6 points of mental ability score damage or penalties.\n\n{b}Construction Requirements:{/b} Craft Wondrous Item, {i}lesser restoration{/i}." compset="Wondrous" summary="Protects against mental ability score effects for 24 hours">
    <fieldval field="gCost" value="750"/>
    <fieldval field="gCraftCost" value="375"/>
    <fieldval field="gWeight" value="1"/>
    <fieldval field="iCL" value="5"/>
    <fieldval field="actUserMin" value="2"/>
    <fieldval field="actUserMax" value="6"/>
    <fieldval field="actIncrNam" value="Doses used"/>
    <fieldval field="trkMax" value="6"/>
    <usesource source="srcDPthMed"/>
    <tag group="gType" tag="Wonder"/>
    <tag group="iSchool" tag="Conjur"/>
    <tag group="Helper" tag="UsesQty"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="Helper" tag="UserAdjust"/>
    <tag group="User" tag="Tracker"/>
    <tag group="Helper" tag="NoNightRes"/>
    <tag group="User" tag="Activation"/>
