  <thing id="raGoWeps" name="Weapon Familiarity" description="You treat all weapons with the word &quot;goblin&quot; in its name as a martial weapon." compset="RaceSpec">
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="Helper" tag="ShowStatbl"/>
    <tag group="WepProf" tag="wPFDogslic"/>
    <tag group="WepProf" tag="wHorsechop"/>
    <eval phase="First"><![CDATA[
      ~ If we're disabled, do nothing
      doneif (tagis[Helper.SpcDisable] <> 0)

      ~assign our weapon proficiencies to the hero
      perform hero.pushtags[WepProf.?]
      perform hero.pushtags[EasyWeapon.?]]]></eval>
    </thing>
  <thing id="raPariah" name="Pariah" description="The appearance of the blue can affect the reactions of those nearby. A blue takes a -1 penalty to Charisma-based skill checks when dealing with nongoblinoid humanoids, but gains a +1 bonus to Charisma-based skill checks while interacting with goblins." compset="RaceSpec">
    <usesource source="pPsiUn"/>
    <tag group="Helper" tag="ShowSpec"/>
    <eval phase="Final" priority="50000"><![CDATA[
      ~ If we're disabled, do nothing
      doneif (tagis[Helper.SpcDisable] <> 0)

      var ChaSkill as number
      ~ Give bonus only to CHA skills
      foreach pick in hero from BaseSkill
        ChaSkill = 0
        ~ Check Skill Override
        if (eachpick.tagis[SkillOver.aCHA] <> 0) then
          ChaSkill = 1
        endif
        ~ If it has a normal linkage to Charisma it also qualifies.
        if (eachpick.islinkage[skillattr] <> 0) then
          if (eachpick.linkage[skillattr].tagis[thingid.aCHA] <> 0) then
            ChaSkill = 1
          endif
        endif
        ~ Yep really a Cha Based Skill
        if (ChaSkill = 1) then
          #situational[eachpick,"-1 penalty vs nongoblinoid humanoids.",field[name].text]
          #situational[eachpick,"+1 bonus vs goblins.",field[name].text]
        endif
      nexteach]]></eval>
    </thing>
  <thing id="raNatPsion" name="Naturally Psionic" description="Members of your race gain the Wild Talent feat as a bonus feat at 1st level. If you take levels in a psionic class, you instead gain the Psionic Talent feat." compset="RaceSpec">
    <tag group="ProductId" tag="HLCommunit"/>
    <bootstrap thing="fPUWildTal">
      <containerreq phase="First" priority="2000"><![CDATA[!Psionics.Class & !AbReplace.raNatPsion]]></containerreq>
      </bootstrap>
    <bootstrap thing="fPUPsiTal">
      <containerreq phase="First" priority="2000"><![CDATA[Psionics.Class & !AbReplace.raNatPsion]]></containerreq>
      </bootstrap>
    <eval phase="First" priority="2000"><![CDATA[
      ~ If we're disabled, do nothing
      doneif (tagis[Helper.SpcDisable] <> 0)

      ~ Set Psionics Race tag on to the hero
      perform hero.assign[Psionics.Race]

      ~ If we are suppose to be assigning the Wild Talent feat we need to set the
      ~ Hero.Psionic tag ontot hero before the feat even runs. So do that here.
      if (tagexpr[!hero#Psionics.Class & !hero#AbReplace.raNatPsion] = 1) then
        ~ Set Hero.Psionic
        call SetHeroPsi
      endif]]></eval>
    </thing>
