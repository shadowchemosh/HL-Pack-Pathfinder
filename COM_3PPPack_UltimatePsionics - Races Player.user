  <thing id="raAberrNat" name="Aberrant Nature" description="Although human in appearance, elans suffer from a nature slightly off from the rest of the non-elan society. They suffer a -1 penalty to Charisma-based skill checks when dealing with non-elans." compset="RaceSpec" summary="-1 to Cha based skill checks for dealing with non-Elans.">
    <tag group="ProductId" tag="HLCommunit"/>
    <eval phase="Final" priority="25000"><![CDATA[
~ If we're disabled, do nothing
doneif (tagis[Helper.SpcDisable] <> 0)

var ChaSkill as number
~ Give bonus only to CHA skills
foreach pick in hero from BaseSkill
  ChaSkill = 0
  ~ Check Skill Override
  if (eachpick.tagis[SkillOver.aCHA] <> 0) then
    ChaSkill = 1
  endif
  ~ If it has a normal linkage to Charisma it also qualifies.
  if (eachpick.islinkage[skillattr] <> 0) then
    if (eachpick.linkage[skillattr].tagis[thingid.aCHA] <> 0) then
      ChaSkill = 1
    endif
  endif
  ~ Yep really a Cha Based Skill
  if (ChaSkill = 1) then
    #situational[eachpick,"-1 penalty vs non-elans.",field[name].text]
  endif
nexteach]]></eval>
    </thing>
