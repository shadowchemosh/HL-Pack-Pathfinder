    <tag group="SpecSource" tag="cHelpMHO"/>
    <eval phase="PostLevel" priority="10500" index="4"><![CDATA[        field[abValue].value += 1

      field[abSumm].text = "+" & field[abValue].value & " to hit and damage with improvised weapons."
      field[livename].text = field[thingname].text & " " & signed(field[abValue].value)
      field[sbName].text = field[thingname].text & " " & signed(field[abValue].value)

      foreach pick in hero from BaseWep where "wSpecial.Improvised"
        eachpick.field[wAttBonus].value += field[abValue].value
        eachpick.field[wDamBonus].value += field[abValue].value
  if (eachpick.field[gIsEquip].value <> 0) then
    hero.child[Initiative].field[Bonus].value += field[abValue].value
  endif
        nexteach]]></eval>
