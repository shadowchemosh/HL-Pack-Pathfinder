      doneif (tagis[Helper.ShowSpec] = 0)

field[abValue].value += field[xCount].value
      field[livename].text = "Heroics Bonus"
      field[abSumm].text = "Your Heroics Point Bonus is +" & field[abValue].value
      field[listname].text = "Heroics Bonus +" & field[xIndex].value

