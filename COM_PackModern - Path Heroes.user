  <thing id="cMHAdaptio" name="Advanced Training - Adaptation" description="A Shadow Hunter learns how to copy the unusual abilities of his prey. He chooses one type of creature he has selected as a favored enemy, such as aberrations. The Shadow Hunter selects one ability or feat from the adaptation list for that type (see below). A Shadow Hunter can use adaptations for 10 minutes per day per  level he possesses. This duration does not need to be consecutive, but it must be used in 10-minute increments. If the adaptation requires the Shadow Hunter to make a more specific choice (such as what skill to use with Skill Focus), this choice is permanent and cannot be changed." compset="CustomSpec" summary="A Shadow Hunter learns how to copy the unusual abilities of his prey.">
    <fieldval field="shortname" value="Adaptation"/>
    <usesource source="D20Arcana"/>
    <tag group="Helper" tag="Secondary"/>
    <tag group="HasAbility" tag="raFavEnemy"/>
    <tag group="User" tag="NeedDomain"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="SpecSource" tag="arMHShdHun"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="ChooseSrc1" tag="Thing"/>
    <tag group="Custom" tag="GRCMdAdvTn" name="Modern Advanced Training" abbrev="Modern Advanced Training"/>
    <tag group="abCategory" tag="GRCMdAdTng" name="Modern Advanced Training" abbrev="Modern Advanced Training"/>
    <eval phase="Level" priority="10000">hero.childfound[cHelpMdH].field[cGiveSp3rd].value += 1</eval>
    </thing>
  <thing id="fMHTtrustC" name="Credible" description="+2 bonus on all Diplomacy checks and Disguise checks.\n\n{b}Benefit{/b}: You gain a +2 bonus on Diplomacy and Disguise.\n\nIf you have 10 or more ranks in one of these skills, the bonus increases to +4 for that skill." compset="Feat" summary="You gain a +2 bonus on Diplomacy and Disguise." uniqueness="unique">
    <fieldval field="shortname" value="Credible"/>
    <usesource source="GRCMP2tMP3"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="BonusFor" tag="Charismati" name="Charismatic Hero" abbrev="Charismatic"/>
    <tag group="fCategory" tag="General"/>
    <eval phase="PreAttr" priority="10000"><![CDATA[     doneif (tagis[Helper.FtDisable] <> 0)

      #skillbonus[skDiplo] += 2
      if (#skillranks[skDiplo] >= 10) then
        #skillbonus[skDiplo] += 2
        endif

      #skillbonus[skDisguise] += 2
      if (#skillranks[skDisguise] >= 10) then
        #skillbonus[skDisguise] += 2
        endif]]></eval>
    </thing>
  <thing id="cACModBN" name="Armor Class Bonus" description="The Hero gains a bonus to their base armor class" compset="ClSpecial" summary="The Hero gains a bonus to their base armor class">
    <usesource source="GRCMP2tMP3"/>
    <tag group="SpecType" tag="Defense"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <eval phase="PostLevel" priority="10000"><![CDATA[      field[listname].text = field[thingname].text & " +" & field[xIndex].value

      ~ only run the rest for the first copy
      doneif (tagis[Helper.FirstCopy] = 0)

      ~ If we're not shown, just get out now
      doneif (tagis[Helper.ShowSpec] = 0)

      ~ If we're disabled, do nothing
      doneif (tagis[Helper.SpcDisable] <> 0)

      field[abValue].value += field[xCount].value
      hero.child[ArmorClass].field[tACDodge].value += field[abValue].value]]></eval>
    </thing>
  <thing id="cFAMEModBN" name="Fame Bonus (Modern Hero)" description="Fame is the total number of Prestige Points earned in a faction. The character can put this Fame bonus in any faction they wish. This bonus will raise your Fame score and Prestige Points in an equal amount. \n\nFor every 10 points of Fame, a character gains a cumulative +1 bonus on Diplomacy checks within that faction." compset="ClSpecial" summary="Fame is the total number of Prestige Points earned in a faction.">
    <usesource source="GRCMP2tMP3"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <eval phase="Final" priority="10500"><![CDATA[      field[listname].text = field[thingname].text & " +" & field[xIndex].value

      ~ only run the rest for the first copy
      doneif (tagis[Helper.FirstCopy] = 0)

      ~ If we're not shown, just get out now
      doneif (tagis[Helper.ShowSpec] = 0)

      ~ If we're disabled, do nothing
      doneif (tagis[Helper.SpcDisable] <> 0)

            if (hero.tagis[source.UCReputat] <> 0) then
      field[abValue].value += field[xCount].value
      hero.child[facReputat].field[facTPA].value += field[abValue].value
endif]]></eval>
    </thing>
  <thing id="cMPJLJBldA" name="Bleeding Attack" description="An Infiltrator with this ability can cause living opponents to bleed by hitting them with a sneak attack. This attack causes the target to take 1 additional point of damage each round for each die of the Infiltrator&apos;s sneak attack (e.g., 3d6 equals 3 points of bleed). Bleeding creatures take that amount of damage every round at the start of each of their turns. The bleeding can be stopped by a DC 15 Heal check or the application of any effect that heals hit point damage. Bleeding damage from this ability does not stack with itself. Bleeding damage bypasses any damage reduction the creature might possess.\n\n{b}Special{/b}: This talent adds an effect to the Infiltrator&apos;s sneak attack. Only one such talent can be applied to an individual attack and the decision must be made before the attack is rolled." compset="CustomSpec" summary="Sneak attack causes extra damage over time." uniqueness="unique">
    <usesource source="GRCMP2tMP3"/>
    <tag group="SpecType" tag="Attack" name="Attack" abbrev="Attack"/>
    <tag group="SpecType" tag="SneakAtt" name="Sneak Attack" abbrev="Sneak Attack"/>
    <tag group="SpecSource" tag="cHelpMSI"/>
    <tag group="Helper" tag="Secondary"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="AbilType" tag="Extra" name="Extraordinary Ability" abbrev=" (Ex)"/>
    <eval phase="Final" priority="15000"><![CDATA[
      ~ our value is the same as our sneak attack die count
      field[abValue].value += #value[cSneakAtt]

      field[livename].text = field[thingname].text & " " & signed(field[abValue].value)
      field[abSumm].text = "Sneak attacks also deal " & field[abValue].value & " bleed damage/round."]]></eval>
    </thing>
