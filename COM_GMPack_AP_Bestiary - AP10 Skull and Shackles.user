var sizemod as number
sizemod = 2
call SizeChange

hero.child[aSTR].field[aStartMod].value = 16
hero.child[aDEX].field[aStartMod].value = -4

perform hero.assign[Custom.tScorpForm]
perform hero.child[wSting].assign[Helper.Keen]

hero.child[wSting].field[wAttBonus].value = 0
hero.child[wSting].field[wDamBonus].value = 1


endif]]></eval>
