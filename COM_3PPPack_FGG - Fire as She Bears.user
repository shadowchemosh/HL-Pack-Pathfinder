    <tag group="Alignment" tag="NeutralLC"/>
    <tag group="Alignment" tag="NeutralGE"/>
    <tag group="Hero" tag="NoDefComm"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="NoScore" tag="aCHA"/>
    <tag group="NoScore" tag="aCON"/>
    <bootstrap thing="raS2BlwWat"></bootstrap>
    <bootstrap thing="raS2RiggNu"></bootstrap>
    <bootstrap thing="raS2SpdWth"></bootstrap>
    <bootstrap thing="raS2SpdInt"></bootstrap>
    <bootstrap thing="raS2ManRat"></bootstrap>
    <bootstrap thing="raS2SpdBas"></bootstrap>
    <bootstrap thing="raS2SpdOvr"></bootstrap>
    <bootstrap thing="raS2ShipTy"></bootstrap>
    <bootstrap thing="raS2Encumb"></bootstrap>
    <bootstrap thing="wS2RamAtt"></bootstrap>
    <bootstrap thing="raS2HullNu"></bootstrap>
    <bootstrap thing="stS2FaSBSh"></bootstrap>
    <bootstrap thing="tpS2Ship"></bootstrap>
    <eval phase="First" priority="10000" index="2"><![CDATA[
      ~ Calculate speed as 2 + Rigging Locations &
      hero.child[Speed].field[tSpeed].value += hero.tagcount[FaSB.Rigging]

      ~ If we have no rigging locations we have speed zero
      If (hero.tagcount[FaSB.Rigging] = 0) Then
         hero.child[Speed].field[tSpeed].value = 0
      Endif]]></eval>
    <eval phase="Final" priority="25000" index="3"><![CDATA[
      ~ Force Mod Speed to be same as base speed
      hero.child[Speed].field[tSpeedMod].value = hero.child[Speed].field[tSpeed].value

      #situational[hero.child[svFort],"Captain's base save value",""]
      #situational[hero.child[svWill],"Captain's base save value",""]
      #situational[hero.child[svRef],"Captain's base save value -2",""]]]></eval>
    <eval phase="First"><![CDATA[
      ~ Strength (Str) = 30 + the number of Hull Locations. &
      ~ A ship’s Strength will be further modified by whether
      ~ you opted to build with Sleek (-4) or Broad (+4) Hulls.
      field[rSTR].value += 20 + hero.tagcount[FaSB.Hull]

      ~ Dexterity (Dex) = 10 + the number of Rigging Locations
      ~ - the number of Hull Locations.
      field[rDEX].value += hero.tagcount[FaSB.Rigging] - hero.child[raS2HullNu].field[domDomain].text

      ~ Hide Unarmed Strike weapon always
      perform hero.child[wUnarmed].assign[Hide.Weapon]

      ~ Hide skills when exported to statblock
      perform hero.child[skBluff].assign[Hide.Statblock]
      perform hero.child[skDiplo].assign[Hide.Statblock]
      perform hero.child[skDisguise].assign[Hide.Statblock]
      perform hero.child[skIntim].assign[Hide.Statblock]]]></eval>
    </thing>
  <thing id="gS2BroaRud" name="Broad Rudder" description="A wide rudder makes a ship more nimble, granting a +1 bonus on all Profession (sailor) checks to maneuver the vessel.\n\n{i}Requirements{/i}: Craft (ships) DC 16" compset="Gear">
