  <thing id="pS2BelWaLi" name="Fire as She Bears: Below Water Line" description="Use this adjustment to increase/decrease the hit points your ship has Below the Water Line." compset="InPlay">
    <usesource source="srcFASB"/>
    <tag group="Helper" tag="AdjConfer"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <eval phase="PreLevel" priority="10000"><![CDATA[
~ If we're not enabled, get out now
doneif (field[pIsOn].value <> 1)

~ Increase/Decrease Below Water Line HP
#trkmax[raS2BlwWat] += field[pAdjust].value]]></eval>
    </thing>
  <thing id="pS2HullHP" name="Fire as She Bears: Hull HP" description="Use this adjustment to increase/decrease the hit points for the selected Hull Locatoin." compset="InPlay">
    <fieldval field="pCandExpr" value="FaSB.Hull"/>
    <usesource source="srcFASB"/>
    <tag group="Adjustment" tag="S2Custom"/>
    <tag group="Helper" tag="AdjConfer"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <eval phase="PreLevel" priority="10000"><![CDATA[
