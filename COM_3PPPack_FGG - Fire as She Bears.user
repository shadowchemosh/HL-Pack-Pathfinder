  <thing id="mecFASB" name="Fire as she Bears Mechanic" compset="Mechanics">
    <usesource source="srcFASB"/>
    <bootstrap thing="raS2FaSB">
      <containerreq phase="First" priority="2000">!IsRace.rS2FasBShp</containerreq>
      </bootstrap>
    <eval phase="First" priority="1000" index="2"><![CDATA[
      ~ Add weight to siege engines for Fire as She Bears
      ~ Loop through all the siege engines
      foreach pick in hero from BaseWep where "wClass.Siege"
        ~ Light Ballista
        If (eachpick.tagis[thingid.wBallistLt] <> 0) Then
          eachpick.field[gWeight].value += 250
        ~ Heavy Ballista
        ElseIf (eachpick.tagis[thingid.wBallistHv] <> 0) Then
          eachpick.field[gWeight].value += 500
        ~ Gate Breaker Ballista
        ElseIf (eachpick.tagis[thingid.wBallisGB] <> 0) Then
          eachpick.field[gWeight].value += 1000
        ~ Light Catapult
        ElseIf (eachpick.tagis[thingid.wCatapulLt] <> 0) Then
          eachpick.field[gWeight].value += 250
        ~ Standard Catapult
        ElseIf (eachpick.tagis[thingid.wCatapulSt] <> 0) Then
          eachpick.field[gWeight].value += 500
        ~ Heavy Catapult
        ElseIf (eachpick.tagis[thingid.wCatapulHv] <> 0) Then
          eachpick.field[gWeight].value += 1000
        ~ Cannon, Fiend's Mouth
        ElseIf (eachpick.tagis[thingid.wCannFiend] <> 0) Then
          eachpick.field[gWeight].value += 2500
        ~ Firedrake
        ElseIf (eachpick.tagis[thingid.wFiredrake] <> 0) Then
          eachpick.field[gWeight].value += 250
        ~ Springal, Arrow
        ElseIf (eachpick.tagis[thingid.wSpringArr] <> 0) Then
          eachpick.field[gWeight].value += 250
        ~ Springal, Arrow
        ElseIf (eachpick.tagis[thingid.wSpringRoc] <> 0) Then
          eachpick.field[gWeight].value += 300
        ~ UC Cannon
        ElseIf (eachpick.tagis[thingid.wCannon] <> 0) Then
          eachpick.field[gWeight].value += 2000
        Endif
        ~ Make it so that a Siege engin can be equipped so it can get
        ~ rolled on the iPad.
        perform eachpick.assign[Helper.AllEqBoth]
      nexteach]]></eval>
    <eval phase="Final" priority="25000"><![CDATA[
      ~ Navel Tatics added to Profession Sailor
      #appenddesc[skProfSail,"{B}NAVAL TACTICS - Fire as She Bears{/B}{br} MANEUVER / DC / SPEED USED{br} {b}Sudden Slow{/b}, 15, varies{br} {b}Sudden Acceleration{/b}, 15, varies{br} {b}45 Degree Turn{/b}, 15, 2{br} {b}90 Degree Turn{/b}, 20, 3{br} {b}180 Degree Turn{/b}, 30, 4{br} {b}Harness Furious Wind{/b}, 20, n/a{br}"]]]></eval>
    <eval phase="Final" priority="50000" index="3"><![CDATA[
      var bonus as number
      bonus = hero.childfound[skKnowEng].field[skTotal].value
      ~ Add weight to siege engines for Fire as She Bears
      ~ Loop through all the siege engines
      foreach pick in hero from BaseWep where "thingid.wCatapulLt|thingid.wCatapulSt|thingid.wCatapulHv"
        eachpick.field[wRanAtk].value = bonus + eachpick.field[wAttBonus].value
      nexteach]]></eval>
    </thing>
  <thing id="gS2ConWePo" name="Concealed Weapon Port" description="The ship&apos;s belowdecks area undergoes major reconstruction in order to house Large direct-fire siege engines, such as light ballistae or cannons, if they are in use in the campaign. A concealed weapon port can only be recognized on a successful DC 15 Perception check. Each concealed port reduces a ship&apos;s cargo capacity by 2 tons, in addition to the space required by the weapon itself.\n\n{i}Requirements{/i}: Craft (ships) DC 16" compset="Gear">
