  <thing id="fS2DragLes" name="Lesser Dragonmark" description="You have a lesser dragonmark.\n\n{b}Prerequisites:{/b} Member of appropriate dragonmarked race and house, Least Dragonmark, 6 ranks in any two skills.\n\n{b}Benefit:{/b} You gain a lesser dragonmark and the use of one spell-like ability associated with the lesser dragonmark of your house (see the next section of this chapter for information on dragonmarks). In addition, you can use your least dragonmark spell-like ability one additional time per day. A saving throw against one of your dragonmark spell-like abilities has a DC of 10 + the level of the spell + your Cha modifier.\n\nYour caster level for your dragonmark spell-like abilities increases to 6 + your level in the dragonmark heir prestige class (if any)." compset="Feat" summary="You have a lesser true dragonmark." uniqueness="useronce">
    <usesource source="S2Eberron"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="Dragonmark" tag="True"/>
    <tag group="Dragonmark" tag="Lesser"/>
    <eval phase="PreLevel" priority="10000"><![CDATA[
      ~ If we're disabled, do nothing
      doneif (tagis[Helper.FtDisable] <> 0)
      ~ If we don't have least feat get out now
      doneif (hero.childlives[fS2DragLea] = 0)
      ~ Put the Dragonmark type and Strength on the hero
      perform hero.pushtags[Dragonmark.?]
      var config as string
      ~ Get the configuators thingid tag
      config = hero.child[fS2DragLea].field[usrChosen1].chosen.tagids[thingid.?,"|"]
      ~ Increase the number Lesser dragonmarks that can be selected by 1
      hero.firstchild[config].field[cfgMax2].value += 1
      ~ Increase the number of uses per day of the Least dragonmark
      foreach pick in hero from BaseSpell where "Dragonmark.Least"
         eachpick.field[trkMax].value += 1
      nexteach]]></eval>
    <eval phase="PostLevel" priority="10000" index="2"><![CDATA[
      ~ If we're disabled, do nothing
      doneif (tagis[Helper.FtDisable] <> 0)

      ~ Increase CL by one of all dragonmark spell-like abilites
      foreach pick in hero from BaseSpell where "Dragonmark.?"
         eachpick.field[sCL].value += 5
      nexteach]]></eval>
    <exprreq message="You have a aberrant dragonmark already.">hero.tagcount[Dragonmark.Aberrant] = 0</exprreq>
    <exprreq message="Least Dragonmark required."><![CDATA[hero.tagis[Dragonmark.Least] <> 0]]></exprreq>
    <exprreq message="Dragonmarked race (human, dwarf, elf, gnome, halfling, half-elf, or half-orc) is required."><![CDATA[#hasrace[rHuman] + #hasrace[rDwarf] + #hasrace[rElf] + #hasrace[rGnome] + #hasrace[rHalfling] + #hasrace[rHalfElf] + #hasrace[rHalfOrc] <> 0]]></exprreq>
    <prereq message="6+ ranks in any two skills required.">
      <validate><![CDATA[
        @valid = 0
        var total as number
        ~ Go through all skills and count the number that have 6+ ranks
        foreach pick in hero from BaseSkill
           If (eachpick.field[skRanks].value >= 6) Then
              total += 1
           Endif
        nexteach
        ~ if we have at least 2 skills at 6 ranks we are valid
        validif (total >= 2)]]></validate>
      </prereq>
    </thing>
