    <exprreq message="You have a aberrant dragonmark already.">hero.tagcount[Dragonmark.Aberrant] = 0</exprreq>
    <exprreq message="Least True Dragonmark required."><![CDATA[hero.tagis[Dragonmark.Least] <> 0]]></exprreq>
