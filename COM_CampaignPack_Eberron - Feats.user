  <thing id="fS2Negotia" name="Negotiator" description="You are good at gauging and swaying attitudes.\n\n{b}Benefit{/b}: You get a +2 bonus on all Diplomacy checks and Sense Motive checks. If you have 10 or more ranks in one of these skills, the bonus increases to +4 for that skill." compset="Feat" summary="You get a +2 bonus on all Diplomacy checks and Sense Motive checks." uniqueness="useronce">
    <fieldval field="abValue" value="2"/>
    <fieldval field="abValue2" value="2"/>
    <usesource source="S2Eberron"/>
    <eval phase="PostLevel" priority="15000"><![CDATA[
      ~ If we're disabled, do nothing
      doneif (tagis[Helper.FtDisable] <> 0)

      if (#skillranks[skDiplo] >= 10) then
         field[abValue].value += 2
      endif

      if (#skillranks[skSenseMot] >= 10) then
         field[abValue2].value += 2
      endif

      #skillbonus[skDiplo] += field[abValue].value
      #skillbonus[skSenseMot] += field[abValue2].value

      ~ Set tag so skills know we came from a +2 bonus feat
      perform hero.childfound[skDiplo].assign[Helper.SklBonFt]
      perform hero.childfound[skSenseMot].assign[Helper.SklBonFt]]]>
      <after name="Suppress Innate Skills"/>
      </eval>
    </thing>
  <thing id="fS2Diligen" name="Diligent" description="Your meticulousness allows you to analyze minute details that others miss.\n\n{b}Benefit{/b}: You get a +2 bonus on all Appraise checks and Linguistics checks.  If you have 10 or more ranks in one of these skills, the bonus increases to +4 for that skill." compset="Feat" summary="You get a +2 bonus on all Appraise checks and Linguistics checks." uniqueness="useronce">
    <usesource source="S2Eberron"/>
    <eval phase="PostLevel" priority="15000"><![CDATA[
      ~ If we're disabled, do nothing
      doneif (tagis[Helper.FtDisable] <> 0)

      #skillbonus[skAppraise] += 2
      if (#skillranks[skAppraise] >= 10) then
         #skillbonus[skAppraise] += 2
      endif

      #skillbonus[skLinguist] += 2
      if (#skillranks[skLinguist] >= 10) then
         #skillbonus[skLinguist] += 2
      endif]]>
      <after name="Suppress Innate Skills"/>
      </eval>
    </thing>
  </document>
