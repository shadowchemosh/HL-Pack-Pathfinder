    <tag group="PathSkill" tag="skHeal" name="Heal" abbrev="Heal"/>
    <tag group="PathSkill" tag="skKnowHist" name="Knowledge (history)" abbrev="Knowledge (history)"/>
    <tag group="SpecSource" tag="arPWaAnces" name="Ancestral" abbrev="Ancestral"/>
    <tag group="ClsAllowPs" tag="puDistrac1" name="Distract" abbrev="Distract"/>
    <tag group="ClsAllowPs" tag="puPreVenW1" name="Prevenom Weapon" abbrev="Prevenom Weapon"/>
    <tag group="ClsAllowPs" tag="puPrevenm1" name="Prevenom" abbrev="Prevenom"/>
    <tag group="Helper" tag="NoPathSoc"/>
    <tag group="ClassSkill" tag="skKnowHist"/>
