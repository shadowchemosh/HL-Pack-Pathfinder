    <tag group="fDisable" tag="fArmHeavy"/>
    <tag group="ClVaryName" tag="AppParen"/>
    <tag group="fDisable" tag="fArmMedium"/>
    <tag group="AbReplace" tag="cPWaPathSk"/>
