    <tag group="ClassType" tag="Normal"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <bootstrap thing="cHelpPU1"></bootstrap>
    <link linkage="helper" thing="cHelpPU1"/>
    </thing>
