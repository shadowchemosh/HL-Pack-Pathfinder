    <bootstrap thing="fFeralComb">
      <containerreq phase="First" priority="2000"><![CDATA[(count:Classes.PsychicWar >= 8) & (fieldval:usrIndex = 2)]]></containerreq>
      <autotag group="ClSpecWhen" tag="8"/>
      <autotag group="thing" tag="skipprereq"/>
