    <usesource source="pPsiUn"/>
    <tag group="cRef" tag="Medium"/>
    <tag group="cWill" tag="Medium"/>
    <tag group="Classes" tag="PsyFist" name="Psychic Fist"/>
    <tag group="fCategory" tag="Psionic"/>
    <tag group="cAttack" tag="Medium"/>
