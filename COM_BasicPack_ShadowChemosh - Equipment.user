    <tag group="Helper" tag="NoSelect"/>
    <tag group="Helper" tag="Shadow"/>

    <tag group="ProductId" tag="HLCommunit"/>
    <eval phase="PreLevel" priority="5000"><![CDATA[
      ~ Manually call HL Gizmo setup for magic & weapons
      call iMagWeapon]]></eval>
    <eval phase="Render" priority="99999999" index="2"><![CDATA[
      ~ Correct the armor razor's name & text which
      ~ is lost when any item power is added.
      Call X1MagName]]></eval>
    <child entity="gMagWeapon">
      <tag group="Helper" tag="NoSelChild"/>
      <bootstrap thing="wS2ArmRazo">
        <autotag group="Helper" tag="Forward"/>
        </bootstrap>
      </child>
    </thing>
