  <thing id="wuTorch" name="Torch" description="If a torch is used in combat, treat it as a one-handed improvised weapon that deals bludgeoning damage equal to that of a gauntlet of its size, plus 1 point of fire damage.{br}{br}{b}Note:{/b} By default this torch is unlit. Please lit the torch to do fire damage from the In-Play tab." compset="Weapon">
    <fieldval field="gSizeCost" value=".01"/>
    <fieldval field="gWeight" value="1"/>
    <usesource source="srcS2Gear"/>
    <tag group="wCritMin" tag="20"/>
    <tag group="wCritMult" tag="2"/>
    <tag group="wSpecial" tag="Improvised"/>
    <tag group="wCategory" tag="Melee"/>
    <tag group="EquipType" tag="Wood"/>
    <tag group="Helper" tag="NormalOnly"/>
    <tag group="wMain" tag="1d3"/>
    <tag group="wProfReq" tag="ImprovMel"/>
    <tag group="wClass" tag="Light"/>
    <tag group="wType" tag="B"/>
    <tag group="Helper" tag="Improvised"/>
    <tag group="Helper" tag="AlwaysAvl"/>
    <tag group="ShadowEqp" tag="TorchLit"/>

    <tag group="ProductId" tag="HLCommunit"/>
    <bootstrap thing="abS2Torch"></bootstrap>
    <eval phase="PreLevel" priority="8000"><![CDATA[
      ~ Feat "Torch Bearer" taken then process here
      If (#hasfeat[fTrchBeare] = 1) Then

        ~ Make us proficient and increase dmg to
        ~ 1d6 as light mace.
        perform assign[Helper.Proficient]
        perform tagreplace[wMain.1d3,wMain.1d6]
      Endif

      ~ Feat "Torch Handling" taken then process here
      If (#hasfeat[fTrchHand] = 1) Then

        ~ Make us a simple weapon
        perform delete[Helper.Improvised]
        perform tagreplace[wProfReq.ImprovMel,wProfReq.Simple]
        field[DescAppend].text &= "{B}Torch Handling{/b} the radius of normal and increased illumination shed by any torch or mundane light source you carry increases by 10 feet."
      Endif]]></eval>
    <eval phase="Final" priority="50000" index="2"><![CDATA[
      ~ If not active get out now! &
      doneif (tagis[ShadowEqp.TorchLit] = 0)

      ~ Show text for fire damage based if we have the feat
      ~ "Burn Burn Burn" or not. Default fire dmg is 1 or
      ~ 1d4 with feat.
      If (#hasfeat[fBurnBurn] = 1) then
        #extradamage[this," plus 1d4 fire",field[name].text]
      Else
        #extradamage[this," plus 1 fire",field[name].text]
      Endif]]></eval>
    </thing>
  <thing id="abS2Torch" name="Torch" description="A torch used in combat is treated as a one-handed improvised weapon that deals bludgeoning damage equal to that of a gauntlet of its size plus 1 point of fire damage." compset="Ability">
