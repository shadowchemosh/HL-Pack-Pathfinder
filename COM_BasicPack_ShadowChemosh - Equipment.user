    <tag group="Helper" tag="ShowSpec"/>
    <tag group="Helper" tag="CoreCampOK"/>

    <tag group="ProductId" tag="HLCommunit"/>
    <eval phase="First" priority="10000"><![CDATA[
      ~ append the proper information to the description text of the spells we modify
      #appenddesc[spNeutPoi3,"{b}Alchemical Power Component: Antitoxin (M){/b}: Add +2 on your caster level check to neutralize poison on a target creature. Antitoxin has no effect when you cast the spell on an object."]
      #appenddesc[spNeutPoi4,"{b}Alchemical Power Component: Antitoxin (M){/b}: Add +2 on your caster level check to neutralize poison on a target creature. Antitoxin has no effect when you cast the spell on an object."]]]></eval>
    <eval phase="Final" priority="50000" index="2"><![CDATA[

      ~ If not activated get out & leave now!
      doneif (field[abilActive].value = 0)

      #situational[hero.child[svFort],"+5 alchemical bonus vs poison.",field[thingname].text]]]></eval>
    </thing>
  <thing id="gS2WealthC" name="Copper Piece" description="This item allows for an easy method of transfering money between characters. You purchase this wealth at cost and trade it to another character. That character can then sell it for 100% of the cost to add the wealth to the character.\n\n{B}Note:{/b} If the &quot;No Coin Weight&quot; option is checked these items will have zero weight on your character." compset="Gear" summary="Copper pieces for transfer among party members." stacking="merge">
