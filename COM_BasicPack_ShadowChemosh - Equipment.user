    <tag group="ProductId" tag="HLCommunit"/>
    <eval phase="Final" priority="10000"><![CDATA[
      ~ If not active get out now!
      doneif (field[abilActive].value = 0)

      if (parent.tagis[component.BaseWep] <> 0) then
        #extradamwithavg[parent," plus 2d6 vicious",field[thingname].text,"7"]
      else
        foreach pick in hero from BaseWep where container.child[gCustMagic].field[gWeapExpr].text
          #extradamwithavg[eachpick," plus 2d6 vicious",field[thingname].text,"7"]
        nexteach
      endif]]></eval>
    <eval phase="Final" priority="50000" index="2"><![CDATA[
      ~ Only proceed if we are a type of ammunition like arrows & bolts
      doneif (parent.tagis[wCategory.Ammunition] = 0)

      ~ We want to always show the activation button as this is really
      ~ useful for arrows/bolts. So even if not equipped show us.
      perform delete[Helper.SpcDisable]
      perform delete[Hide.Activation]]]></eval>
    <eval phase="Render" priority="111000" index="3"><![CDATA[
      ~ Set a nice looking activation name &
      field[actName].text = field[abSource].text & " (+2d6, 1d6 to self)"]]></eval>
    </thing>
  <thing id="gS2Antitox" name="Antitoxin" description="This substance counteracts a specific toxin. If you drink a vial of antitoxin, you gain a +5 alchemical bonus on Fortitude saving throws against poison for 1 hour.\n\n{b}Alchemical Power Component{/b}\nLike antiplague, this substance can augment certain healing spells.\n{b}Neutralize Poison (M){/b}: Add +2 on your caster level check to neutralize poison on a target creature. Antitoxin has no effect when you cast the spell on an object.\n\n{b}Note:{/b} This gear item allows for activation ability on the In-Play tab." compset="Gear" stacking="merge">
    <fieldval field="gCost" value="50"/>
    <fieldval field="gCraftDC" value="25"/>
    <fieldval field="actName" value="Drink"/>
    <usesource source="srcS2Gear"/>
    <tag group="gType" tag="SpecSubs"/>
    <tag group="CraftSkill" tag="skCrafAlch"/>
    <tag group="User" tag="Activation"/>
