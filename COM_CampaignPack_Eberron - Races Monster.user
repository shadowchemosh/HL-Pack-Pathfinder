  <thing id="raS2PoiPar" name="Poison - Paralysis" description="You have a poison attack." compset="RaceSpec">
    <fieldval field="abValue2" value="1"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="SpecType" tag="Attack" name="Attack" abbrev="Attack"/>
    <tag group="StandardDC" tag="aCON" name="Constitution" abbrev="Constitution"/>
    <tag group="AbilType" tag="Extra" name="Extraordinary Ability" abbrev=" (Ex)"/>
    <eval phase="Final" priority="20000"><![CDATA[
field[abSumm].text = "Poison paralyzes for " & field[abDuration].value & " round(s)"

if (field[abValue2].value = 1) then
    field[abSumm].text &= ", cure 1 save."
elseif (field[abValue2].value > 1) then
    field[abSumm].text &= ", cure " & field[abValue2].value & " consecutive saves."
endif

var sTags as string
~ Get all Is Weapon tags
sTags = tagids[IsWeapon.?,"|"]

~ Set extra damage note on all primary natural attacks
foreach pick in hero from BaseWep where sTags
   #extradamage[eachpick," plus poison",field[name].text]
nexteach]]></eval>
    </thing>
