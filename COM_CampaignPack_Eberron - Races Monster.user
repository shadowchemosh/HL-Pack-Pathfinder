    <child entity="wSpecMagic">
      <bootstrap thing="wWhip">
        <autotag group="Helper" tag="TwoWpLight"/>
        </bootstrap>
      </child>
    </thing>
  <thing id="raS2ChTouc" name="Channel Touch" description="A tentacle whip can be used to deliver a touch attack - either a natural power of the bearer or a held magical charge." compset="RaceSpec" summary="Deliver touch attacks through the tentacle whip.">
