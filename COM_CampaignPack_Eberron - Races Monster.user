  <thing id="raS2AciAtt" name="Acidic Attack" description="A horrid animal&apos;s primary attack deals an extra 1d6 points of acid damage per 4 HD of the horrid animal (maximum 5d6). A horrid animal&apos;s secondary attacks do not deal acid damage." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="SpecType" tag="Attack"/>
    <tag group="AbilType" tag="Extra"/>
    <eval phase="Final" priority="20000"><![CDATA[
var sText as string
~ Calc the plus 1d6 per 4HD
field[abValue].value += round(herofield[tHitDice].value/4,0,-1)
~ If less than 1 make it 1
If (field[abValue].value < 1) Then
   field[abValue].value = 1
~.. if greater than 5 make it 5
elseIf (field[abValue].value > 5) Then
   field[abValue].value = 5
endif
sText = "+" & field[abValue].value  & "d6 acid"
~ Set extra damage note on all primary natural attacks
foreach pick in hero from BaseWep where "wGroup.Natural & Helper.NatPrimary"
   #extradamage[eachpick,sText,field[name].text]
nexteach

field[abSumm].text = "Your primary natural attacks do +" & field[abValue].value & "d6 acid damage."]]></eval>
    </thing>
