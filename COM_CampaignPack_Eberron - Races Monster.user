  <thing id="tmS2HorAni" name="Horrid Animal" description="&quot;Horrid animal&quot; is an inherited template that can be added to any dire animal (referred to hereafter as the base creature)." compset="Template" uniqueness="unique">
    <fieldval field="tmCON" value="4"/>
    <fieldval field="tmCRMin" value="1"/>
