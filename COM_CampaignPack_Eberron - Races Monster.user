  <thing id="xDamRdByes" name="Damage Reduction, Byeshk" description="You have Damage Reduction against all except byeshk attacks." compset="Ability" uniqueness="unique">
    <usesource source="S2Eberron"/>
    <tag group="SpecType" tag="DR"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="NaturalDR" tag="Adamant"/>
    <tag group="HasAbility" tag="xDamRdAny"/>
    <tag group="Helper" tag="BaseAbil"/>
    <tag group="Universal" tag="Universal"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <eval phase="Final" priority="10000"><![CDATA[
      ~ Set abValue
      call CalcValue
      ~ add in abValue2
      field[abValue].value += field[abValue2].value
      var sText as string
      ~ make this easier to setup with only a single DR type string
      sText = "byeshk"
      ~ Use our value field as the amount of damage we can resist
      field[livename].text = "Damage Reduction (" & field[abValue].value & "/" & sText & ")"
      field[shortname].text = field[abValue].value & "/" & sText]]></eval>
    </thing>
