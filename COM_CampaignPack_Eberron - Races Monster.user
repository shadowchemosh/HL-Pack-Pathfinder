  <thing id="raS2BaseSp" name="Based on Spell" description="Pick the spell that this Living Spell is based on. This will be used to control many aspects of the race." compset="RaceSpec">
    <fieldval field="usrCandid1" value="component.BaseSpell &amp; sClass.cHelpWiz"/>
    <tag group="ChooseSrc1" tag="Thing"/>
    <eval phase="First" priority="600"><![CDATA[~ If we have not chosen then get out now
doneif (field[usrChosen1].ischosen <> 1)

~ set our focus to the hero's race
perform hero.findchild[BaseRace].setfocus
doneif (state.isfocus = 0)

~ set our race's name to our own name
focus.field[livename].text = "Living " &  field[usrChosen1].chosen.field[name].text

~ Get the spell level
perform field[usrChosen1].chosen.pulltags[sLevel.?]
~ fill in the spell level to abValue
field[abValue].value += tagvalue[sLevel.?]

~ CR = spell level plus half the caster level rounded down
focus.field[rCR].value += field[abValue].value

~ Set are ability scores
focus.field[rSTR].value += field[abValue].value
focus.field[rCON].value += field[abValue].value
focus.field[rCHA].value += field[abValue].value
focus.field[rDEX].value += field[abValue].value - 3
focus.field[rWIS].value += field[abValue].value - 3]]></eval>
    <eval phase="PreLevel" priority="10000" index="2">~ set SR
#applysr[10+field[abValue].value]
~ Resistance bonus on saves
#applybonus[svResist,hero.child[svAll],field[abValue].value]
~ Deflection bonus to AC equal to spell level
#applybonus[tACDeflect,hero.child[ArmorClass],field[abValue].value]
~ Incrase Spell Effect by Spell Level
hero.childfound[raS2SpEffe].field[abDC].value += field[abValue].value
~ Change Spell Effect name
hero.childfound[raS2SpEffe].field[livename].text = field[usrChosen1].chosen.field[name].text</eval>
    </thing>
  <thing id="raS2CasLev" name="Caster Level" description="Pick the caster level of the spell which controls the living spells size." compset="RaceSpec">
    <fieldval field="usrCandid1" value="component.Simple &amp; LivingSp.SelectSize"/>
    <tag group="ChooseSrc1" tag="Thing"/>
    <eval phase="First"><![CDATA[~ If we have not chosen then get out now
doneif (field[usrChosen1].ischosen <> 1)

~ set our focus to the hero's race
perform hero.findchild[BaseRace].setfocus
doneif (state.isfocus = 0)

~ Pull the Race Size from the selector
perform field[usrChosen1].chosen.pulltags[RaceSize.?]
~ Push the Race Size to the base race
perform focus.pushtags[RaceSize.?]]]></eval>
    </thing>
  <thing id="rcS2LiSpCL" name="Caster Level" description="Add this so you can set the caster level of your living spell." compset="RaceCustom" summary="Type in the CL." uniqueness="unique">
    <tag group="User" tag="NeedDomain"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="AllowRCust" tag="rS2LivSpel"/>
    <eval phase="First"><![CDATA[
~set our focus to the hero's race
perform hero.findchild[BaseRace].setfocus
doneif (state.isfocus = 0)

~ Set the Caster Level and calculate init HP
field[abValue].value += field[domDomain].text
field[abValue2].value += round(4.5 * field[abValue].value,0,-1)

~ CR = spell level plus half the caster level rounded down
focus.field[rCR].value += round(field[abValue].value/2,0,-1)

~ Set hit dice
focus.field[rHitDice].value += field[abValue].value

~ Give message about HP amount
If (field[abValue].value <> 0) Then
   field[abSumm].text = "Starting Hit Points is " & field[abValue2].value
Endif

~ Based on CL set our size
If (field[abValue].value >= 13) Then
   perform focus.assign[RaceSize.Huge2]
ElseIf (field[abValue].value >= 7) Then
   perform focus.assign[RaceSize.Large1]
Else
   perform focus.assign[RaceSize.Medium0]
Endif]]></eval>
    </thing>
  <thing id="raS2SpEffe" name="Spell Effect" description="A creature hit by a living spell&apos;s slam attack is subjected to the normal effect of the spell or spells making up the creature." compset="RaceSpec">
