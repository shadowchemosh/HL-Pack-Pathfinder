    <tag group="mLevel" tag="5"/>
    <bootstrap thing="wPWElmBlst">
      <containerreq phase="First" priority="450"><![CDATA[fieldVal:abilActive = 1 & hero#PWActNRG.Air]]></containerreq>
      <autotag group="wType" tag="Elec"/>
      </bootstrap>
    <bootstrap thing="xFly">
      <containerreq phase="First" priority="450"><![CDATA[fieldVal:abilActive = 1 & hero#PWActNRG.Air]]></containerreq>
      <autotag group="Maneuver" tag="Good"/>
      </bootstrap>
    <bootstrap thing="wPWElmBlst">
      <containerreq phase="First" priority="450"><![CDATA[fieldVal:abilActive = 1 & hero#PWActNRG.Fire]]></containerreq>
      <autotag group="wType" tag="Fire"/>
      </bootstrap>
    <bootstrap thing="wPWElmBlst">
      <containerreq phase="First" priority="450"><![CDATA[fieldVal:abilActive = 1 & hero#PWActNRG.Water]]></containerreq>
      <autotag group="wType" tag="Cold"/>
      </bootstrap>
    <bootstrap thing="wPWElmBlst">
      <containerreq phase="First" priority="450"><![CDATA[fieldVal:abilActive = 1 & hero#PWActNRG.Earth]]></containerreq>
      <autotag group="wType" tag="Acid"/>
      </bootstrap>
    <bootstrap thing="xSwim">
      <containerreq phase="First" priority="450"><![CDATA[fieldVal:abilActive = 1 & hero#PWActNRG.Water]]></containerreq>
      </bootstrap>
    <bootstrap thing="stAquatic">
      <containerreq phase="First" priority="450"><![CDATA[fieldVal:abilActive = 1 & hero#PWActNRG.Water]]></containerreq>
      </bootstrap>
    <bootstrap thing="raAmphib">
      <containerreq phase="First" priority="450"><![CDATA[fieldVal:abilActive = 1 & hero#PWActNRG.Water]]></containerreq>
      </bootstrap>
    <bootstrap thing="xBurrow">
      <containerreq phase="First" priority="450"><![CDATA[fieldVal:abilActive = 1 & hero#PWActNRG.Earth]]></containerreq>
      </bootstrap>
    <bootstrap thing="xTremorsen">
      <containerreq phase="First" priority="450"><![CDATA[fieldVal:abilActive = 1 & hero#PWActNRG.Earth]]></containerreq>
      </bootstrap>
    <eval phase="PostAttr" priority="10000"><![CDATA[      ~ If we're not enabled, get out now
      doneif (field[abilActive].value = 0)
      ~ Stop now if we don't have an active element
      doneif (hero.tagcount[PWActNRG.?] = 0)

      ~ Air:
      if (hero.tagis[PWActNRG.Air] = 1) then
        #value[xFly] += hero.child[Speed].field[tSpeed].value
      endif

      ~ Earth:
      if (hero.tagis[PWActNRG.Earth] = 1) then
        #value[xBurrow] += hero.child[Speed].field[tSpeed].value
        #value[xTremorsen] += field[abValue].value
      endif

      ~ Water: Swim speed, subtypes
      if (hero.tagis[PWActNRG.Water] = 1) then
        #value[xSwim] += hero.child[Speed].field[tSpeed].value * 2
      endif

      ~ Fire: Init bonus, base speed bonus
      if (hero.tagis[PWActNRG.Fire] = 1) then
        hero.child[Initiative].field[Bonus].value += field[abValue2].value
        hero.child[Speed].field[tSpeed].value += field[abValue3].value
      endif]]></eval>
    </thing>
  <thing id="staPWMotE8" name="Master of the Elements" description="You attune your very form to roiling elemental energies and merge with them. While you maintain this stance, your type changes to outsider (native), with elemental subtype of your active element. In addition, you are treated as if you were under the effects of the {i}elemental body III spell{/i}. You may continue to wield weapons, wear armor, and use other items while in this form, and your new body does not damage or otherwise negatively affect your equipment.\n\n{b}NOTE:{/b} To get the effect of this stance, you will need to go to the Adjustments tab and add the {i}elemental body III{/i} adjustment under Spell Adjustments.  The stance script is not currently capable of doing this automatically." compset="PWManuever" summary="Take on elemental aspects based on active element." uniqueness="useronce">
