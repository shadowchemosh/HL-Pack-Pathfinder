    <tag group="mRange" tag="Personal"/>
    <tag group="mTarget" tag="You"/>
    <tag group="mDuration" tag="Stance"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="PathOfWar" tag="Stance"/>
    <bootstrap thing="fComRef">
      <containerreq phase="First" priority="100"><![CDATA[fieldVal:abilActive <> 0]]></containerreq>
      </bootstrap>
    <eval phase="PostAttr" priority="10000"><![CDATA[      ~ If we're not enabled, get out now
      doneif (field[abilActive].value = 0)

      ~ Combat Reflexes: Use initiatior attribute instead of DEX
      hero.child[fComRef].field[abValue].value = 1 + maximum(field[mIM].value, 0)]]></eval>
    </thing>
  <thing id="staPWRcBl1" name="Reaching Blade Stance" description="By relying on the momentum generated in making an attack, the Mithral Current Disciple can extend their reach to strike opponents further away than they could normally reach.  While in this stance, your attacks deal an extra 1d6 points of damage plus an additional 1d6 points of damage per 8 levels and the first attack made per round can attack an opponent 5 ft further away." compset="PWManuever" uniqueness="useronce">
    <fieldval field="abValue" value="1"/>
    <fieldval field="abValue2" value="6"/>
    <usesource source="srcPOWEx"/>
    <tag group="mLevel" tag="1"/>
    <tag group="mInitAct" tag="Swift1"/>
    <tag group="mRange" tag="Personal"/>
    <tag group="mTarget" tag="You"/>
    <tag group="mDuration" tag="Stance"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="PathOfWar" tag="Stance"/>
    <tag group="mType" tag="Stance"/>
    <tag group="Discipline" tag="MithCurr"/>
    <eval phase="PostAttr" priority="10000"><![CDATA[      ~ If we're not enabled, get out now
      doneif (field[abilActive].value = 0)

      ~ Calculate bonus damage
      field[abValue].value += round(field[mIL].value/8,0,-1)

      ~ Add bonus damage to attacks
      foreach pick in hero from BaseWep
        #extradamage[eachpick, signed(field[abValue].value) & "d" & field[abValue2].value, field[name].text]
      nexteach]]></eval>
    </thing>
  <thing id="staPWFlWa3" name="Flowing Water Stance" description="The disciple adopts a stance that maximizes free movement and  speed, allowing them to dance around their opponents attacks and flow past obstacles like a rushing stream.  While in this stance the disciple gains a +4 dodge bonus to AC and CMD and a +4 bonus to initiative.  In addition, the disciple may sheathe their weapon once per round as a free action (even if it is not their turn) that does not provoke attacks of opportunity." compset="PWManuever" summary="+4 to dodge AC and init, can sheate 1/rd with no AoO" uniqueness="useronce">
    <comment>abValue = Dodge bonus
abValue2 = Init bonus</comment>
    <fieldval field="abValue2" value="4"/>
    <fieldval field="abValue" value="4"/>
    <usesource source="srcPOWEx"/>
    <tag group="mInitAct" tag="Swift1"/>
