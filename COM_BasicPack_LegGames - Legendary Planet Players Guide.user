  <thing id="rcX1BreWep" name="Breath Weapon" description="Choose one of the following energy types: acid, cold, electricity, or fire. If the creature is an outsider (native), it must have ties to an elemental plane, and it must pick an energy that corresponds to the plane it has ties to (acid [earth], cold [water], electricity [air], or fire [fire]). Then pick either a 15-foot cone or a 20-foot line. Once per day, as a standard action, members of this race can make a supernatural breath weapon attack that deals 1d6 points of the damage type chosen in the area chosen. All creatures within the affected area must make a Reflex saving throw to avoid taking damage. The save DC against this breath weapon is 10 + 1/2 the user’s character level + the user’s Constitution modifier. Those who succeed at the save take no damage from the attack. \n\n{b}Special{/b}: You can take this trait more than once. Each time you do so, the cost of this trait increases by 1 RP. When do, you can augment the breath weapon in the following ways (augmentations marked with an asterisk [*] can be taken more than once).\nExtra Breath*: The member of this race can use its breath weapon an additional time per day.\nIncreased Area: Increase the cone’s size to 30 feet or the line to 50 feet.\nIncreased Damage*: Increase the damage by an additional d6.\nPowerful Breath: The breath weapon deals half damage on a failed saving throw." compset="RaceCustom" uniqueness="useronce">
    <fieldval field="usrCandid2" value="component.SelectHelp &amp; PackSelect.BreWepShB2"/>
    <usesource source="srcLegPlAP"/>
    <tag group="AllowRCust" tag="rX1Auttain"/>
    <tag group="fShowWhat" tag="EnergyBase"/>
    <tag group="ChooseSrc2" tag="Thing"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomCost" tag="1"/>
    <bootstrap thing="xCBBreathW">
      <autotag group="BreathSve" tag="Reflex"/>
      <autotag group="Usage" tag="Day"/>
      <autotag group="SaveEff" tag="Negates"/>
      <autotag group="HasAbility" tag="rcX1BreWep"/>
      <assignval field="abValue" value="1"/>
      <assignval field="abValue2" value="6"/>
      <assignval field="trkMax" value="1"/>
      </bootstrap>
    <eval phase="PreLevel" priority="10000"><![CDATA[
      ~ If nothing chosen get out now!
      doneif (field[usrChosen1].ischosen = 0)
      doneif (field[usrChosen2].ischosen = 0)

      ~ Pull Energy Type
      perform field[usrChosen1].chosen.pulltags[BloodEner.?]
      ~ Pull Line or Cone
      perform field[usrChosen2].chosen.pulltags[BreathShp.?]

      perform hero.findchild[RaceAbil,"thingid.xCBBreathW & HasAbility.rcX1BreWep"].setfocus
      doneif (state.isfocus = 0)

      ~ Push Energy Type and Breath Weapon Shape to breath weapon helper
      perform focus.pushtags[BloodEner.?]
      perform focus.pushtags[BreathShp.?]

      ~ If cone shape give 15ft if line make us 20ft
      if (tagis[BreathShp.Cone] = 1) then
        focus.field[abRange].value += 15
      else
        focus.field[abRange].value += 20
      endif]]></eval>
    </thing>
  <thing id="rcX1BrWeEx" name="Breath Weapon, Extra Breath" description="The member of this race can use its breath weapon an additional time per day.\n\n{b}Special{/b}: This counts as Breath Weapon, and you can take this trait more than once." compset="RaceCustom" summary="Gain another use of a breath weapon.">
    <usesource source="srcLegPlAP"/>
    <tag group="AllowRCust" tag="rX1Auttain"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomCost" tag="1"/>
    <eval phase="First" priority="10000"><![CDATA[
      ~ Set pointer to the breath weapon ability
      perform hero.findchild[RaceAbil,"thingid.xCBBreathW & HasAbility.rcX1BreWep"].setfocus
      doneif (state.isfocus = 0)

      ~ Give one extra use of breath weapon
      focus.field[trkMax].value += 1]]></eval>
    <exprreq message="Breath Weapon ability required."><![CDATA[#hasability[rcX1BreWep] <> 0]]></exprreq>
    </thing>
  <thing id="rcX1BrWeIA" name="Breath Weapon, Increased Area" description="Increase the cone’s size to 30 feet or the line to 50 feet.\n\n{b}Special{/b}: This counts as the Breath Weapon trait." compset="RaceCustom" summary="Increase the area of effect of your breath weapon." uniqueness="useronce">
    <usesource source="srcLegPlAP"/>
    <tag group="AllowRCust" tag="rX1Auttain"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomCost" tag="1"/>
    <eval phase="PreLevel" priority="10000"><![CDATA[
      ~ Set pointer to the breath weapon ability
      perform hero.findchild[RaceAbil,"thingid.xCBBreathW & HasAbility.rcX1BreWep"].setfocus
      doneif (state.isfocus = 0)

      ~ Increase cone by 15ft and line by 30ft
      if (focus.tagis[BreathShp.Cone] = 1) then
        focus.field[abRange].value += 15
      else
        focus.field[abRange].value += 30
      endif]]></eval>
    <exprreq message="Breath Weapon ability required."><![CDATA[#hasability[rcX1BreWep] <> 0]]></exprreq>
    </thing>
  <thing id="rcX1BrWeID" name="Breath Weapon, Increased Damage" description="The member of this race increases its breath weapond damage by an additional 1d6.\n\n{b}Special{/b}: This counts as Breath Weapon, and you can take this trait more than once." compset="RaceCustom" summary="Increase damage by another 1d6.">
    <usesource source="srcLegPlAP"/>
    <tag group="AllowRCust" tag="rX1Auttain"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomCost" tag="1"/>
    <eval phase="First" priority="10000"><![CDATA[
      ~ Set pointer to the breath weapon ability
      perform hero.findchild[RaceAbil,"thingid.xCBBreathW & HasAbility.rcX1BreWep"].setfocus
      doneif (state.isfocus = 0)

      ~ Give one dice of damage to breath weapon
      focus.field[abValue].value += 1]]></eval>
    <exprreq message="Breath Weapon ability required."><![CDATA[#hasability[rcX1BreWep] <> 0]]></exprreq>
    </thing>
  <thing id="rcX1BrWePo" name="Breath Weapon, Powerful" description="Those that make the save vs your breath weapon still take half-damage.\n\n{b}Special{/b}: This counts as the Breath Weapon trait." compset="RaceCustom" summary="Half damage to those that make the save." uniqueness="useronce">
    <usesource source="srcLegPlAP"/>
    <tag group="AllowRCust" tag="rX1Auttain"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomCost" tag="1"/>
    <eval phase="PreLevel" priority="10000"><![CDATA[
      ~ Set pointer to the breath weapon ability
      perform hero.findchild[RaceAbil,"thingid.xCBBreathW & HasAbility.rcX1BreWep"].setfocus
      doneif (state.isfocus = 0)

      ~ Change to half damage on a successful save
      perform focus.delete[SaveEff.Negates]
      perform focus.assign[SaveEff.Half]]]></eval>
    <exprreq message="Breath Weapon ability required."><![CDATA[#hasability[rcX1BreWep] <> 0]]></exprreq>
    </thing>
  <thing id="raX1BuildP" name="Build Points" description="Auttaine receive a number of build points equal to 3 + their initial Constitution modifier, which they may use to customize the artificial components comprising their half-construct nature. These build points may be spent on a one for one basis as additional racial points (RPs) as defined in Chapter 4 of the {i}Pathfinder RPG Advanced Race Guide{/i}. Auttaine may only spend these RPs on the following enhancements: breath weapon, desert runner, energy resistance, fleet-footed, gifted linguist, jumper, natural armor, skill bonus, skill training, sprinter, stability, weapon familiarity. In addition, auttaine may also select from the special racial traits as part of their gear.\n\n{b}Note:{/b} Hero Lab can not track your characters starting Constiution modifier. Enter in your characters&apos;s starting Con modifer.  In example if your starting Con score is 12 then you would type in 1 for build points." compset="RaceSpec" summary="Add special artificial components to your character.">
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="User" tag="NeedDomain"/>
    <eval phase="First" priority="10000">~ Attempt to add in the number of the Con Modifier
      ~ the base races number of specials
      root.field[rGiveSpec].value += field[domDomain].text</eval>
    </thing>
  <thing id="rcX1ConWep" name="Concealed Weapon" description="This artificial component provides an internal, deployable weapon contained within a single arm. It may only include a light melee weapon weighing less than 3 lbs, and the initial weapon cannot be made from any special materials. While internalized, the weapon receives a +4 bonus on Sleight of Hand checks to conceal it. The weapon can be changed with 4 hours of work and a new qualifying weapon.\n\n{b}Note:{/b} Add the weapon that is concealed on the Weapons tab and then select it from the drop down list." compset="RaceCustom" summary="Conceal a 3lb light melee weapon which receives +4 bonus on Slight of Hand checks." uniqueness="useronce">
    <usesource source="srcLegPlAP"/>
    <tag group="AllowRCust" tag="rX1Auttain"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="fShowWhat" tag="WeaponsMel"/>
    <tag group="ChooseSrc1" tag="Hero"/>
    <tag group="CustomCost" tag="2"/>
    </thing>
  <thing id="rcX1HiStCo" name="Hidden Storage Compartment" description="This artificial component provides an internal storage compartment within the torso, holding up to 1 cubic foot of material. Access is secured with a simple combination lock (DC 20 Disable Device to open).\n\n{b}Note:{/b} Add the gear item concealed on the Gear tab and then select it from the drop down list." compset="RaceCustom" summary="Conceal a 1 cubic ft of material that is secure with a DC20 lock." uniqueness="useronce">
    <fieldval field="usrCandid1" value="component.MyGear"/>
    <usesource source="srcLegPlAP"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="fShowWhat" tag="Freeform"/>
    <tag group="ChooseSrc1" tag="Hero"/>
    <tag group="CustomCost" tag="1"/>
    <tag group="AllowRCust" tag="rX1Auttain"/>
    </thing>
  <thing id="rcX1InScPo" name="Integrated Spell Component Pouch" description="This artificial component provides an internal storage compartment in one of the auttaine&apos;s hands which acts as a spell component pouch. Special slots in each finger allows the use of material components stored within even when they have no free hands." compset="RaceCustom" summary="An internal storage compartment in one hand acts as a spell component pouch." uniqueness="useronce">
    <usesource source="srcLegPlAP"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="CustomCost" tag="2"/>
    <tag group="AllowRCust" tag="rX1Auttain"/>
    </thing>
  <thing id="rcX1IntToo" name="Integrated Toolset" description="This artificial component provides a customized toolset for granting a +2 circumstance bonus to one of the following skills: Climb, Craft, Disable Device, Escape Artist, Heal, Perception, Perform, Swim.\n\n{b}Note:{/b} Add the gear item concealed on the Gear tab and then select it from the drop down list." compset="RaceCustom" summary="Artificial component acts as a tool granting a +2 circumstance bonus to the skill." uniqueness="useronce">
    <fieldval field="usrCandid1" value="component.BaseSkill &amp;  !Helper.Obsolete &amp; (Helper.SkCatCraft|Helper.SkCatPerf|thingid.skClimb|thingid.skDisable|thingid.skEscape|thingid.skHeal|thingid.skPercep|thingid.skSwim)"/>
    <fieldval field="abValue" value="2"/>
    <usesource source="srcLegPlAP"/>
    <tag group="fShowWhat" tag="Freeform"/>
    <tag group="ChooseSrc1" tag="Hero"/>
    <tag group="CustomCost" tag="2"/>
    <tag group="AllowRCust" tag="rX1Auttain"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <eval phase="PreLevel" priority="10000">~ If nothing chosen get out now!
      doneif (field[usrChosen1].ischosen = 0)

      ~ Give a +2 circumstance bonus to the skill selected
      perform field[usrChosen1].chosen.field[skToolMod].modify[+,field[abValue].value,field[thingname].text]

      ~ We need to set the Tools Available tag to the hero
      ~ to let the skill know a tool is present.
      perform field[usrChosen1].chosen.forward[ToolsAvail.?]</eval>
    </thing>
  <thing id="rcX1DesRun" name="Desert Runner" description="Members of this race receive a +4 racial bonus on Constitution checks and Fortitude saves to avoid fatigue and exhaustion, as well as any other ill effects from running, forced marches, starvation, thirst, and hot or cold environments." compset="RaceCustom" summary="+4 to Con checks and Fort saves vs fatigue/exhaustion, or ill effects from running, starvation, and temperature." uniqueness="useronce">
    <usesource source="srcLegPlAP"/>
    <tag group="CustomCost" tag="2"/>
    <tag group="AllowRCust" tag="rX1Auttain"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="SpecType" tag="Defense"/>
    <eval phase="Final" priority="50000"><![CDATA[
      ~ If disabled get out now!
      doneif (tagis[Helper.SpcDisable] = 1)

      #situational[hero.child[svFort],"+4 vs. fatigue, exhaustion, running, forced marches, starvation, thirst, and hot or cold environments",field[name].text]
      #situational[hero.child[aCON],"+4 vs. fatigue, exhaustion, running, forced marches, starvation, thirst, and hot or cold environments",field[name].text]]]></eval>
    </thing>
  <thing id="rcX1EneRes" name="Energy Resistance" description="Pick one of the following energy types that corresponds to the dragon type or plane the race has ties to: acid (earth), cold (water), electricity (air), or fire (fire). Members of this race have resistance 5 to the corresponding energy type. \n\n{b}Special{/b}: This trait can be taken more than once. Each time it is taken, select an additional energy type that corresponds to another dragon type or elemental plane the race has ties to." compset="RaceCustom" summary="Energy resistance 5.">
    <fieldval field="abValue" value="5"/>
    <usesource source="srcLegPlAP"/>
    <tag group="AllowRCust" tag="rX1Auttain"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="SpecType" tag="Defense"/>
    <tag group="CustomCost" tag="2"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="fShowWhat" tag="EnergyBase"/>
    <evalrule phase="PostLevel" priority="10000" message="We can&apos;t be vulnerable to the same energy type we have resistance/immunity to!" summary="We can&apos;t be vulnerable to the same energy type we have resistance/immunity to!"><![CDATA[
      ~ If we are disabled get out now!
      validif (tagis[Helper.SpcDisable] = 1)

      if (field[usrChosen1].chosen.tagis[BloodEner.Acid] = 1) then
        #applyresist[xDamRsAcid,field[abValue].value]

        ~ We can't be vulnerable to the same energy type we have resistance/immunity to!
        validif (hero.tagis[Custom.VulnAcid] = 0)
        @message = "You can't have immunity/resistance against acid if you have acid vulnerability!"

      elseif (field[usrChosen1].chosen.tagis[BloodEner.Cold] <> 0) then
        #applyresist[xDamRsCold,field[abValue].value]

        ~ We can't be vulnerable to the same energy type we have resistance/immunity to!
        validif (hero.tagis[Custom.VulnCold] = 0)
        @message = "You can't have immunity/resistance against cold if you have cold vulnerability!"

      elseif (field[usrChosen1].chosen.tagis[BloodEner.Elec] <> 0) then
        #applyresist[xDamRsElec,field[abValue].value]

        ~ We can't be vulnerable to the same energy type we have resistance/immunity to!
        validif (hero.tagis[Custom.VulnElec] = 0)
        @message = "You can't have immunity/resistance against electricity if you have electricity vulnerability!"

      elseif (field[usrChosen1].chosen.tagis[BloodEner.Fire] <> 0) then
        #applyresist[xDamRsFire,field[abValue].value]

        ~ We can't be vulnerable to the same energy type we have resistance/immunity to!
        validif (hero.tagis[Custom.VulnFire] = 0)
        @message = "You can't have immunity/resistance against fire if you have fire vulnerability!"
      endif]]></evalrule>
    </thing>
  <thing id="rcX1FleFoo" name="Fleet-Footed" description="Members of this race receive Run as a bonus feat and a +2 racial bonus on initiative checks." compset="RaceCustom" uniqueness="useronce">
    <fieldval field="abValue" value="2"/>
    <usesource source="srcLegPlAP"/>
    <tag group="AllowRCust" tag="rX1Auttain"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="SpecType" tag="Defense"/>
    <tag group="CustomCost" tag="3"/>
    <tag group="Helper" tag="SpecUp"/>
    <bootstrap thing="fRun"></bootstrap>
    <eval phase="PostAttr" priority="10000"><![CDATA[
      ~ if we've been replaced, get out now
      doneif (tagis[Helper.SpcDisable] <> 0)

      hero.child[Initiative].field[Bonus].value += field[abValue].value]]></eval>
    </thing>
  <thing id="rcX1GifLin" name="Gifted Linguist" description="Members of this race gain a +4 racial bonus on Linguistics checks, and they learn one additional language every time they put a rank in the Linguistics skill." compset="RaceCustom" uniqueness="useronce">
    <fieldval field="abValue" value="4"/>
    <usesource source="srcLegPlAP"/>
    <tag group="AllowRCust" tag="rX1Auttain"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="CustomCost" tag="2"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="SpecType" tag="Skill"/>
    <eval phase="PostAttr" priority="5000" index="2"><![CDATA[
      ~if we've been replaced, get out now
      doneif (tagis[Helper.SpcDisable] <> 0)

      herofield[tLangsSpk].value += #skillranks[skLinguist]]]></eval>
    <eval phase="PreLevel" priority="4000"><![CDATA[
      ~if we've been replaced, get out now
      doneif (tagis[Helper.SpcDisable] <> 0)

      #racialbonus[skLinguist] += field[abValue].value]]></eval>
    </thing>
