  <thing id="rcX1SkilB1" name="Skill Bonus (One at +2)" description="Pick a single skill. Members of this race gain a +2 racial bonus on skill checks made with this skill. Alternatively, pick two related skills—each member of this race gains a +1 racial bonus on these skills during character creation.\n\n{b}Special{/b}: This trait can be taken up to three times. Each time it is taken, choose a different skill (+2 bonus) or two different skills (+1 bonus on one of character’s choice)." compset="RaceCustom" uniqueness="useronce">
    <fieldval field="abValue" value="2"/>
    <usesource source="srcLegPlAP"/>
    <tag group="AllowRCust" tag="rX1Auttain"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="CustomCost" tag="2"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="SpecType" tag="Skill"/>
    <tag group="fShowWhat" tag="Skills"/>
    <tag group="ChooseSrc1" tag="Hero"/>
    <eval phase="PreLevel" priority="10000">~ If we are disabled get out now!
      doneif (tagis[Helper.SpcDisable] = 1)
      ~ If nothing chosen get out now!
      doneif (field[usrChosen1].ischosen = 0)

      perform hero.assign[Custom.SkillBon]

      field[usrChosen1].chosen.field[Bonus].value += field[abValue].value
      perform field[usrChosen1].chosen.assign[Helper.CustChosen]</eval>
    <exprreq message="You may only choose this option 3 times."><![CDATA[hero.tagcount[Custom.SkillBon] - @ispick <= 2]]></exprreq>
    </thing>
  <thing id="rcX1SkilB2" name="Skill Bonus (Two at +1)" description="Pick a single skill. Members of this race gain a +2 racial bonus on skill checks made with this skill. Alternatively, pick two related skills—each member of this race gains a +1 racial bonus on these skills during character creation.\n\n{b}Special{/b}: This trait can be taken up to three times. Each time it is taken, choose a different skill (+2 bonus) or two different skills (+1 bonus on one of character’s choice)." compset="RaceCustom" uniqueness="useronce">
    <fieldval field="abValue" value="1"/>
    <usesource source="srcLegPlAP"/>
    <tag group="AllowRCust" tag="rX1Auttain"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="CustomCost" tag="2"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="SpecType" tag="Skill"/>
    <tag group="fShowWhat" tag="Skills"/>
    <tag group="ChooseSrc1" tag="Hero"/>
    <tag group="fShowWhat2" tag="Skills"/>
    <tag group="ChooseSrc2" tag="Hero"/>
    <eval phase="PreLevel" priority="10000">~ If we are disabled get out now!
      doneif (tagis[Helper.SpcDisable] = 1)
      ~ If nothing chosen get out now!
      doneif (field[usrChosen1].ischosen = 0)
      doneif (field[usrChosen2].ischosen = 0)

      perform hero.assign[Custom.SkillBon]

      field[usrChosen1].chosen.field[Bonus].value += field[abValue].value
      perform field[usrChosen1].chosen.assign[Helper.CustChosen]
      field[usrChosen2].chosen.field[Bonus].value += field[abValue].value
      perform field[usrChosen2].chosen.assign[Helper.CustChosen]</eval>
    <exprreq message="You may only choose this option 3 times."><![CDATA[hero.tagcount[Custom.SkillBon] - @ispick <= 2]]></exprreq>
    </thing>
  <thing id="rcX1SkiTra" name="Skill Training" description="Pick up to two skills. These skills are always considered class skills for members of this race." compset="RaceCustom" uniqueness="useronce">
    <usesource source="srcLegPlAP"/>
    <tag group="AllowRCust" tag="rX1Auttain"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="CustomCost" tag="2"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="fShowWhat" tag="Skills"/>
    <tag group="ChooseSrc1" tag="Hero"/>
    <tag group="fShowWhat2" tag="Skills"/>
    <tag group="ChooseSrc2" tag="Hero"/>
    <eval phase="PreLevel" priority="10000">~ If we are disabled get out now!
      doneif (tagis[Helper.SpcDisable] = 1)
      ~ If nothing chosen get out now!
      doneif (field[usrChosen1].ischosen = 0)
      doneif (field[usrChosen2].ischosen = 0)

      ~ Make the selected skills class skills
      perform field[usrChosen1].chosen.assign[Helper.ClassSkill]
