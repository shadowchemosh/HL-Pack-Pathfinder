  <thing id="cRPOBBOutc" name="Outcast" description="{b}Benefit{/b}: An Outcast gains a +4 Dexterity bonus. They tend to be very good at disappearing." compset="ClSpecial" summary="An Outcast gains a +4 Dexterity bonus.">
    <usesource source="RPOBldBlde"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <eval phase="PreAttr" priority="10000">hero.child[aDEX].field[aNormMod].value += 4</eval>
    </thing>
  <thing id="cRPOBBForg" name="Forgettable" description="{b}Benefit{/b}: Outcasts are never noticed by anyone, including slashers. When presented with other targets, the Outcast is always the last to be attacked. What were we talking about again?" compset="ClSpecial" summary="Outcasts are never noticed by anyone, including slashers.">
    <usesource source="RPOBldBlde"/>
    <tag group="ProductId" tag="HLCommunit"/>
    </thing>
  <thing id="cRPOBBQuic" name="Quick" description="{b}Benefit{/b}: Outcasts receive Combat Expertise or Dodge as a bonus feat." compset="ClSpecial" summary="Gain Combat Expertise or Dodge as a bonus feat.">
    <arrayval field="usrArray" index="0" value="Combat Expertise"/>
    <arrayval field="usrArray" index="1" value="Dodge"/>
    <usesource source="RPOBldBlde"/>
    <tag group="abDuration" tag="Permanent"/>
    <tag group="abRange" tag="Personal"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="Custom" tag="RPOBBComEx" name="Combat Expertise" abbrev="Combat Expertise"/>
    <tag group="Custom" tag="RPOBBDodge" name="Dodge" abbrev="Dodge"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <tag group="abAction" tag="Personal"/>
    <bootstrap thing="fDodge">
      <containerreq phase="First" priority="200">hero#Custom.RPOBBDodge</containerreq>
      <autotag group="thing" tag="skipprereq"/>
      </bootstrap>
    <bootstrap thing="fComExpert">
      <containerreq phase="First" priority="200">hero#Custom.RPOBBComEx</containerreq>
      <autotag group="thing" tag="skipprereq"/>
      </bootstrap>
    <eval phase="First">if (field[usrIndex].value = 0) then
      perform hero.assign[Custom.RPOBBComEx]
     
      elseif (field[usrIndex].value = 1) then
      perform hero.assign[Custom.RPOBBDodge]
      endif</eval>
    </thing>
  <thing id="cRPOBBDece" name="Deceptive" description="{b}Benefit{/b}: Outcasts use their Intelligence modifier for Bluff checks instead of Charisma. Outcasts double their Dexterity modifier for all Stealth checks." compset="ClSpecial" summary="Use INT mod for Bluff/ Double Dex mod for Stealth checks.">
    <usesource source="RPOBldBlde"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="ProductId" tag="HLCommunit"/>
    <eval phase="PostAttr" priority="10000">field[abValue].value += #attrbonus[aDEX]


#skillbonus[skStealth] += field[abValue].value</eval>
    <eval phase="First" priority="10000" index="2">perform hero.child[skBluff].assign[SkillOver.aINT]</eval>
    </thing>
  <thing id="cRPOBBUnDg" name="Uncanny Dodge" description="{b}Benefit{/b}: An Outcast can react to danger before her normal senses would typically allow her to do so. She retains her Dexterity bonus to AC (if any) even if she is caught flat-footed or struck by an invisible attacker. However, she still loses her Dexterity bonus to AC if immobilized." compset="ClSpecial" summary="Retains her Dexterity bonus to AC even if flat-footed.">
    <usesource source="RPOBldBlde"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="ProductId" tag="HLCommunit"/>
    </thing>
  <thing id="cRPOBBHPSt" name="Hide in Plain Sight" description="{b}Benefit{/b}: An Outcast can use the Stealth skill even while being observed. In fact, one is hiding behind you right now…" compset="ClSpecial" summary="Use the Stealth skill even while being observed.">
