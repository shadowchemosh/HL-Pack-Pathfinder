    <tag group="Helper" tag="Mythic"/>
    <exprreq message="Mythic Tier 3 required."><![CDATA[#tierprereq[] >= 3]]></exprreq>
    </thing>
  <thing id="cPUMBTele" name="Block Telepathy" description="As an immediate action, you can spend one use of mythic power to gain immunity to mind-affecting effects for one round.\n\n{b}Note:{/b} This ability is not yet implemented." compset="CustomSpec" uniqueness="useronce">
    <comment>Not implemented</comment>
    <usesource source="srcMyPsi"/>
    <tag group="AbilType" tag="Super"/>
