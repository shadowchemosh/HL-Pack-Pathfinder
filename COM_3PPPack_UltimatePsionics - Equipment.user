      ~ If activated, add insight bonus to AC
      if (field[abilActive].value <> 0) then
        ~ Add 1 to our chosen skill
        focus.field[BonComp].value += 1
      endif

      if (isgearheld <> 0) then
        if (gearholder.tagis[Custom.SynMask] <> 0 ) then
