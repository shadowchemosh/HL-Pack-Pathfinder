    <tag group="Helper" tag="AlwaysMast"/>
    <tag group="Helper" tag="Proficient"/>
    <tag group="Helper" tag="AutoEquip"/>
    <eval phase="PreLevel" priority="10000"><![CDATA[
      ~ Work out if we're equipped, or, if we're the child of a magic item, if
      ~ our parent is equipped
      if (container.ishero <> 0) then
        if (field[gIsEquip].value <> 0) then
          perform hero.assign[Hero.TowAtkPen2]
          endif
      else
        if (parent.field[gIsEquip].value <> 0) then
          perform hero.assign[Hero.TowAtkPen2]
          endif
        endif]]>
      <before name="Calc wTowerPen"/>
      </eval>
    </thing>
  <thing id="ioPUBraDis" name="Bracers of Disruption" description="These black silk bands increase the base damage of the wielder’s disrupt pattern ability by 2d6 points of damage. If the character does not have the Disrupt Pattern class feature, he gains no benefit.\n\n{b}Construction Requirements{/b} \nCraft Wondrous Item, disrupt pattern class feature, {b}Cost{/b} 10,000 gp" compset="Wondrous">
